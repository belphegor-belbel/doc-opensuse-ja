      "bridge.forward-delay:                   15\n"
      "bridge.hello-time:                      2\n"
      "bridge.max-age:                         20\n"
      "bridge.ageing-time:                     300\n"
      "bridge.group-forward-mask:              0\n"
      "bridge.multicast-snooping:              yes\n"
      "bridge.vlan-filtering:                  no\n"
