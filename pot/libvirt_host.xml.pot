      " Name       MAC address          Protocol     Address\n"
      "-------------------------------------------------------------------------------\n"
      " vnet0      52:54:00:9e:0d:2b    ipv6         fd00:dead:beef:55::140/64\n"
      " -          -                    ipv4         192.168.100.168/24"
