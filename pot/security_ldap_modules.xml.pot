#. Tag: title
#: security_ldap_modules.xml:9
#, no-c-format
msgid "Managing modules"
msgstr ""

#. Tag: para
#: security_ldap_modules.xml:10
#, no-c-format
msgid "Use the following command to list all available modules, enabled and disabled. Use your server's hostname rather than the instance name of your &ds389; server, like the following example hostname of <replaceable>LDAPSERVER1</replaceable> :"
msgstr ""

#. Tag: screen
#: security_ldap_modules.xml:16
#, no-c-format
msgid "\n"
      "&prompt.sudo;<command>dsconf -D \"cn=Directory Manager\" ldap://<replaceable>LDAPSERVER1</replaceable> plugin list</command>\n"
      "Enter password for cn=Directory Manager on ldap://<replaceable>LDAPSERVER1</replaceable>: <replaceable>PASSWORD</replaceable>\n"
      "\n"
      "7-bit check\n"
      "Account Policy Plugin\n"
      "Account Usability Plugin\n"
      "ACL Plugin\n"
      "ACL preoperation\n"
      "[...]"
msgstr ""

#. Tag: para
#: security_ldap_modules.xml:26
#, no-c-format
msgid "The following command enables the MemberOf plugin referenced in <xref linkend=\"sec-security-ldap-server-sssd\"/> :"
msgstr ""

#. Tag: screen
#: security_ldap_modules.xml:30
#, no-c-format
msgid "\n"
      "&prompt.sudo;<command>dsconf -D \"cn=Directory Manager\" ldap://<replaceable>LDAPSERVER1</replaceable> plugin memberof enable</command>\n"
      ""
msgstr ""

#. Tag: para
#: security_ldap_modules.xml:33
#, no-c-format
msgid "Note that the plugin names used in commands are lowercase, so they are different from how they appear when you list them. If you make a mistake with a plugin name, you will see a helpful error message:"
msgstr ""

#. Tag: screen
#: security_ldap_modules.xml:39
#, no-c-format
msgid "dsconf instance plugin: error: invalid choice: 'MemberOf' (choose from\n"
      "'memberof', 'automember', 'referential-integrity', 'root-dn', 'usn',\n"
      "'account-policy', 'attr-uniq', 'dna', 'linked-attr', 'managed-entries',\n"
      "'pass-through-auth', 'retro-changelog', 'posix-winsync', 'contentsync', 'list',\n"
      "'show', 'set')"
msgstr ""

#. Tag: para
#: security_ldap_modules.xml:44
#, no-c-format
msgid "After enabling a plugin, it is necessary to restart the server:"
msgstr ""

#. Tag: screen
#: security_ldap_modules.xml:47
#, no-c-format
msgid "\n"
      "&prompt.sudo;<command>systemctl restart dirsrv@<replaceable>LDAPSERVER1</replaceable>.service</command>\n"
      ""
msgstr ""

#. Tag: para
#: security_ldap_modules.xml:50
#, no-c-format
msgid "To avoid having to restart the server, set the <literal>nsslapd-dynamic-plugins</literal> parameter to <literal>on</literal> :"
msgstr ""

#. Tag: screen
#: security_ldap_modules.xml:55
#, no-c-format
msgid "\n"
      "&prompt.sudo;<command>dsconf -D \"cn=Directory Manager\" ldap://<replaceable>LDAPSERVER1</replaceable> config replace \\</command>\n"
      "<command>nsslapd-dynamic-plugins=on</command>\n"
      "Enter password for cn=Directory Manager on ldap://LDAPSERVER1: <replaceable>PASSWORD</replaceable>\n"
      "\n"
      "Successfully replaced \"nsslapd-dynamic-plugins\""
msgstr ""

