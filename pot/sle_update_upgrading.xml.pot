msgstr ""

#. Tag: para
#: sle_update_upgrading.xml:169
#, no-c-format
msgid "<xref linkend=\"cha.upgrade-online\"/>"
msgstr ""

#. Tag: term
#: sle_update_upgrading.xml:177
#, no-c-format
msgid "Upgrading from &opensuse; Leap 15"
msgstr ""

#. Tag: para
#: sle_update_upgrading.xml:179
#, no-c-format
