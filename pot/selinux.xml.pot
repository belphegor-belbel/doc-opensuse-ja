      "/sbin/init                      system_u:object_r:bin_t:s0 -&gt; \\\n"
      "                                system_u:object_r:init_exec_t:s0\n"
      "/usr/sbin/sshd                  system_u:object_r:sshd_exec_t:s0"
msgstr ""

#. Tag: para
#: selinux.xml:424
#, no-c-format
msgid "If you are not able to boot the server properly with &selnx; in enforcing mode, switch back to permissive mode. Check the log files with <command>less /var/log/audit/audit.log</command> . For more details, see <xref linkend=\"sec-selinux-troubleshoot\"/> ."
msgstr ""

