msgid "<literal>-parallel <replaceable>DEVICE</replaceable></literal>"
msgstr ""

#. Tag: para
#: qemu_running_vms_qemukvm.xml:1157
#, no-c-format
msgid "Redirects the &vmguest;'s parallel port to a <replaceable>DEVICE</replaceable> . This option supports the same devices as <literal>-serial</literal> ."
