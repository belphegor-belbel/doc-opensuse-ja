msgid "Specifies the host name that the built-in DHCP server will assign to &vmguest;."
msgstr ""

#. Tag: title
#: qemu_running_vms_qemukvm.xml:1407
#, no-c-format
msgid "User-mode Networking with Network-boot and TFTP"
msgstr ""

#. Tag: screen
#: qemu_running_vms_qemukvm.xml:1408
#, no-c-format
msgid "&prompt.sudo;qemu-system-x86_64 [...] \\\n"
      "-netdev user,id=hostnet0 \\\n"
      "-device virtio-net-pci,netdev=hostnet0,tftp=/images/tftp_dir<co xml:id=\"co-usermode-tftp\"/>,\\\n"
      "bootfile=/images/boot/pxelinux.0<co xml:id=\"co-usermode-bootfile\"/>"
