msgstr ""

#. Tag: title
#: rmt_migrate_from_smt.xml:745
#, no-c-format
msgid "Importing &smt; data to &rmt;"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:748
#, no-c-format
msgid "To make sure your &rmt; installation is up to date, run <command>zypper up</command> ."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:754
#, no-c-format
msgid "Copy the exported <literal>.tar.gz</literal> file to an empty directory and unpack it. Then enter the new directory:"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:758
#, no-c-format
msgid "&prompt.user;<command>mkdir <replaceable>EMPTY_DIR</replaceable></command>\n"
      "&prompt.user;<command>cd <replaceable>EMPTY_DIR</replaceable></command>\n"
      "&prompt.user;<command>tar xf <replaceable>/PATH/TO/</replaceable>smt-export.<replaceable>TIMESTAMP</replaceable>.tar.gz</command>\n"
      "&prompt.user;<command>cd smt-export</command>\n"
      ""
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:765
#, no-c-format
msgid "If you chose to export the SSL certificates from &smt;, copy the CA private key and certificate to <filename>/etc/rmt/ssl/</filename> :"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:770
#, no-c-format
msgid "&prompt.sudo;<command>cp ssl/cacert.key /etc/rmt/ssl/rmt-ca.key</command>\n"
      "&prompt.sudo;<command>cp ssl/cacert.pem /etc/rmt/ssl/rmt-ca.crt</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:774
#, no-c-format
msgid "Run the &yast; &rmt; configuration module as described in <xref linkend=\"sec-rmt-installation-yast-configuration\"/> . If you imported the &smt; CA certificate, add the domain of the &smt; server to the common names of the new SSL certificate."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:782
#, no-c-format
msgid "Run the &rmt; synchronization to get the products and repositories data from &scc;."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:786
#, no-c-format
msgid "&prompt.sudo;<command>rmt-cli sync</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:789
#, no-c-format
msgid "Import the data from the &smt; server."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:792
#, no-c-format
msgid "&prompt.sudo;<command>rmt-data-import -d ./</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:795
#, no-c-format
msgid "Optional: If the URL of the &rmt; server changed, change the URL parameter of clients in <filename>/etc/SUSEConnect</filename> to point to the new &rmt; server. Alternatively, change the DNS records to re-assign the host name to the &rmt; server."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:803
#, no-c-format
msgid "Optional: Move the mirrored repository data from &smt; to &rmt;, and adjust the ownership of the copied data."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:807
#, no-c-format
msgid "&prompt.sudo;<command>cp -r /var/www/htdocs/repo/* /usr/share/rmt/public/repo/</command>\n"
      "&prompt.sudo;<command>chown -R _rmt:nginx /usr/share/rmt/public/repo</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:810
#, no-c-format
msgid "The path for storing custom repositories' data on the &rmt; server is different from that of &smt;. With &rmt;, it replicates the directory structure of the source server's URL into a top level directory. For example, if the URL of the custom repository is"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:816
#, no-c-format
msgid "http://download.opensuse.org/debug/distribution/leap/&productnumbershort;/repo/oss"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:817
#, no-c-format
msgid "its path on the &rmt; server will be"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:820
#, no-c-format
msgid "/usr/share/rmt/public/repo/debug/distribution/leap/&productnumbershort;/repo/oss"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:824
#, no-c-format
msgid "Custom repositories on the &smt; server are disabled be default. If you want to mirror them to the &rmt; enable them before mirroring."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:830
#, no-c-format
msgid "Check for custom repositories by running:"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:833
#, no-c-format
msgid "&prompt.sudo;<command>rmt-cli repos custom list</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:834
#, no-c-format
msgid "A table of all custom repositories will be shown. The first column contains the <literal>ID</literal> of each repository and the <literal>Mirror?</literal> column will show <literal>false</literal> ."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:841
#, no-c-format
msgid "Enable each custom repository you want to mirror by running:"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:844
#, no-c-format
msgid "&prompt.sudo;<command>rmt-cli repos custom enable <replaceable>ID</replaceable></command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:849
#, no-c-format
msgid "Update the packages in the repositories by starting the mirroring process:"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:852
#, no-c-format
msgid "&prompt.sudo;<command>rmt-cli mirror</command>"
msgstr ""

