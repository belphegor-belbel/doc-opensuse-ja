#. Tag: para
#: rmt_migrate_from_smt.xml:831
#, no-c-format
msgid "Copy the exported <literal>.tar.gz</literal> file to an empty directory and unpack it. Then enter the new directory:"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:835
#, no-c-format
msgid "&prompt.user;<command>mkdir <replaceable>EMPTY_DIR</replaceable></command>\n"
      "&prompt.user;<command>cd <replaceable>EMPTY_DIR</replaceable></command>\n"
      "&prompt.user;<command>tar xf <replaceable>/PATH/TO/</replaceable>smt-data-export.<replaceable>TIMESTAMP</replaceable>.tar.gz</command>\n"
      "&prompt.user;<command>cd smt-data-export</command>\n"
      ""
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:842
#, no-c-format
msgid "If you chose to export the SSL certificates from &smt;, copy the CA private key and certificate to <filename>/etc/rmt/ssl/</filename> :"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:846
#, no-c-format
msgid "&prompt.sudo;<command>cp ssl/cacert.key /etc/rmt/ssl/rmt-ca.key</command>\n"
      "&prompt.sudo;<command>cp ssl/cacert.pem /etc/rmt/ssl/rmt-ca.crt</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:850
#, no-c-format
msgid "Run the &yast; &rmt; configuration module as described in <xref linkend=\"sec-rmt-installation-yast-configuration\"/> . If you imported the &smt; CA certificate, add the domain of the &smt; server to the common names of the new SSL certificate."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:858
#, no-c-format
msgid "Run the &rmt; synchronization to get the products and repositories data from &scc;."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:862
#, no-c-format
msgid "&prompt.sudo;<command>rmt-cli sync</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:865
#, no-c-format
msgid "Import the data from the &smt; server."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:868
#, no-c-format
msgid "&prompt.sudo;<command>rmt-data-import -d ./</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:871
#, no-c-format
msgid "Optional: if the URL of the &rmt; server changed, change the URL parameter of clients in <filename>/etc/SUSEConnect</filename> to point to the new &rmt; server. Alternatively, change the DNS records to re-assign the host name to the &rmt; server."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:879
#, no-c-format
msgid "Optional: move the mirrored repository data from &smt; to &rmt;, and adjust the ownership of the copied data."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:883
#, no-c-format
msgid "&prompt.sudo;<command>cp -r /var/www/htdocs/repo/* /usr/share/rmt/public/repo/</command>\n"
      "&prompt.sudo;<command>chown -R _rmt:nginx /usr/share/rmt/public/repo</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:886
#, no-c-format
msgid "The path for storing custom repository data on the &rmt; server is different from that of &smt;. With &rmt;, it replicates the directory structure of the source server's URL into a top level directory structure of the source server's URL into a top-level"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:892
#, no-c-format
msgid "http://download.opensuse.org/debug/distribution/leap/&productnumber-leaprepo;/repo/oss"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:893
#, no-c-format
msgid "its path on the &rmt; server corresponds to"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:896
#, no-c-format
msgid "/usr/share/rmt/public/repo/debug/distribution/leap/&productnumber-leaprepo;/repo/oss"
msgstr ""

