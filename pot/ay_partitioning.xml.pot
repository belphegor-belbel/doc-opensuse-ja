      "        &lt;size&gt;20G&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;raid_name&gt;/dev/md/0&lt;/raid_name&gt;\n"
      "        &lt;size&gt;max&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "    &lt;use&gt;all&lt;/use&gt;\n"
      "  &lt;/drive&gt;\n"
      "  &lt;drive&gt;\n"
      "    &lt;device&gt;/dev/sdb&lt;/device&gt;\n"
      "    &lt;disklabel&gt;none&lt;/disklabel&gt;\n"
      "    &lt;partitions config:type=\"list\"&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;raid_name&gt;/dev/md/0&lt;/raid_name&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "    &lt;use&gt;all&lt;/use&gt;\n"
      "  &lt;/drive&gt;\n"
      "  &lt;drive&gt;\n"
      "    &lt;device&gt;/dev/md/0&lt;/device&gt;\n"
      "    &lt;partitions config:type=\"list\"&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;mount&gt;/home&lt;/mount&gt;\n"
      "        &lt;size&gt;40G&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;mount&gt;/srv&lt;/mount&gt;\n"
      "        &lt;size&gt;10G&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "    &lt;raid_options&gt;\n"
      "      &lt;chunk_size&gt;4&lt;/chunk_size&gt;\n"
