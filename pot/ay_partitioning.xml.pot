#. Tag: screen
#: ay_partitioning.xml:2318
#, no-c-format
msgid "&lt;dev_name&gt;/dev/dasda&lt;/dev_name&gt;"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:2319
#, no-c-format
