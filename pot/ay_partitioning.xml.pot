      "        &lt;/raid_options&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "    &lt;use&gt;all&lt;/use&gt;\n"
      "  &lt;/drive&gt;\n"
      "&lt;/partitioning&gt;"
msgstr ""

#. Tag: title
#: ay_partitioning.xml:2184
#, no-c-format
