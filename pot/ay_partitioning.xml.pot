#. Tag: para
#: ay_partitioning.xml:1824
#, no-c-format
msgid "For RAID6 and RAID10, the following values can be used:"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1827
#, no-c-format
msgid "<literal>parity_first</literal> , <literal>parity_last</literal> , <literal>left_asymmetric_6</literal> , <literal>left_symmetric_6</literal> , <literal>right_asymmetric_6</literal> , <literal>right_symmetric_6</literal> , <literal>parity_first_6</literal> , <literal>n2</literal> , <literal>o2</literal> , <literal>f2</literal> , <literal>n3</literal> , <literal>o3</literal> , or <literal>f3</literal> ."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1836
#, no-c-format
msgid "&lt;parity_algorithm&gt;left_asymmetric&lt;/parity_algorithm&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:1840
#, no-c-format
msgid "raid_type"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1842
#, no-c-format
msgid "Possible values are: <literal>raid0</literal> , <literal>raid1</literal> , <literal>raid5</literal> , <literal>raid6</literal> and <literal>raid10</literal> ."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1847
#, no-c-format
msgid "&lt;raid_type&gt;raid1&lt;/raid_type&gt;"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1848
#, no-c-format
msgid "The default is <literal>raid1</literal> ."
msgstr ""

#. Tag: term
#: ay_partitioning.xml:1854
#, no-c-format
msgid "device_order"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1856
#, no-c-format
msgid "This list contains the order of the physical devices:"
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1859
#, no-c-format
msgid "&lt;device_order config:type=\"list\"&gt;&lt;device&gt;/dev/sdb2&lt;/device&gt;&lt;device&gt;/dev/sda1&lt;/device&gt;...&lt;/device_order&gt;"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1860
#, no-c-format
msgid "This is optional, and the default is alphabetical order."
msgstr ""

#. Tag: title
#: ay_partitioning.xml:1872
#, no-c-format
msgid "Multipath support"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1873
#, no-c-format
msgid "&ay; can handle multipath devices. To take advantage of them, you need to enable multipath support, as shown in <xref linkend=\"multipath-example\"/> . Alternatively, you can use the following parameter on the Kernel command line: <literal>LIBSTORAGE_MULTIPATH_AUTOSTART=ON</literal> ."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1881
#, no-c-format
msgid "Unlike &sle; 12, it is not required to set the drive section type to <literal>CT_DMMULTIPATH</literal> . You should use <literal>CT_DISK</literal> , although for historical reasons, both values are equivalent."
msgstr ""

#. Tag: title
#: ay_partitioning.xml:1888
#, no-c-format
msgid "Using multipath devices"
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1889
#, no-c-format
msgid "<![CDATA[<general>\n"
      "  <storage>\n"
      "    <start_multipath config:type=\"boolean\">true</start_multipath>\n"
      "  </storage>\n"
      "</general>\n"
      "<partitioning>\n"
      "  <drive>\n"
      "    <partitions config:type=\"list\">\n"
      "      <partition>\n"
      "        <size>20G</size>\n"
      "        <mount>/</mount>\n"
      "        <filesystem config:type=\"symbol\">ext4</filesystem>\n"
      "      </partition>\n"
      "      <partition>\n"
      "        <size>auto</size>\n"
      "        <mount>swap</mount>\n"
      "      </partition>\n"
      "    </partitions>\n"
      "    <type config:type=\"symbol\">CT_DISK</type>\n"
      "    <use>all</use>\n"
      "  </drive>\n"
      "</partitioning>]]>"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1912
#, no-c-format
msgid "If you want to specify the device, you could use the World Wide Identifier (WWID), its device name (for example, <literal>/dev/dm-0</literal> ), any other path under <literal>/dev/disk</literal> that refers to the multipath device or any of its paths."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1918
#, no-c-format
msgid "For example, given the <literal>multipath</literal> listing from <xref linkend=\"multipath-ll-output\"/> , you could use <literal>/dev/mapper/14945540000000000f86756dce9286158be4c6e3567e75ba5</literal> , <literal>/dev/dm-3</literal> , any other corresponding path under <literal>/dev/disk</literal> (as shown in <xref linkend=\"multipath-wwid-example\"/> ), or any of its paths ( <literal>/dev/sda</literal> or <literal>/dev/sdb</literal> )."
msgstr ""

#. Tag: title
#: ay_partitioning.xml:1930
#, no-c-format
msgid "Listing multipath devices"
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1931
#, no-c-format
msgid "<![CDATA[# multipath -l\n"
      "14945540000000000f86756dce9286158be4c6e3567e75ba5 dm-3 ATA,VIRTUAL-DISK\n"
      "size=40G features='0' hwhandler='0' wp=rw\n"
      "|-+- policy='service-time 0' prio=1 status=active\n"
      "| `- 2:0:0:0 sda 8:0  active ready running\n"
      "`-+- policy='service-time 0' prio=1 status=enabled\n"
      "  `- 3:0:0:0 sdb 8:16 active ready running]]>"
msgstr ""

#. Tag: title
#: ay_partitioning.xml:1940
#, no-c-format
msgid "Using the WWID to identify a multipath device"
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1941
#, no-c-format
msgid "<![CDATA[<drive>\n"
      "  <partitions config:type=\"list\">\n"
      "    <device>/dev/mapper/14945540000000000f86756dce9286158be4c6e3567e75ba5</device>\n"
      "    <partition>\n"
      "      <size>20G</size>\n"
      "      <mount>/</mount>\n"
      "      <filesystem config:type=\"symbol\">ext4</filesystem>\n"
      "    </partition>\n"
      "  </partitions>\n"
      "  <type config:type=\"symbol\">CT_DISK</type>\n"
      "  <use>all</use>\n"
      "</drive>]]>"
msgstr ""

#. Tag: title
#: ay_partitioning.xml:1957
#, no-c-format
msgid "&bcache; configuration"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1958
#, no-c-format
msgid "&bcache; is a caching system which allows the use of multiple fast drives to speed up the access to one or more slower drives. For example, you can improve the performance of a large (but slow) drive by using a fast one as a cache."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1964
#, no-c-format
msgid "For more information about bcache on &productname;, also see the blog post at <link xlink:href=\"https://www.suse.com/c/combine-the-performance-of-solid-state-drive-with-the-capacity-of-a-hard-drive-with-bcache-and-yast/\"/> ."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1969
#, no-c-format
msgid "To set up a &bcache; device, &ay; needs a profile that specifies the following:"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1975
#, no-c-format
msgid "To set a (slow) block device as <emphasis>backing device</emphasis> , use the <literal>bcache_backing_for</literal> element."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1981
#, no-c-format
msgid "To set a (fast) block device as <emphasis>caching device</emphasis> , use the <literal>bcache_caching_for</literal> element. You can use the same device to speed up the access to several drives."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1988
#, no-c-format
msgid "To specify the layout of the &bcache; device, use a <literal>drive</literal> section and set the <literal>type</literal> element to <literal>CT_BCACHE</literal> . The layout of the &bcache; device may contain partitions."
msgstr ""

#. Tag: title
#: ay_partitioning.xml:1997
#, no-c-format
msgid "&bcache; definition"
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1998
#, no-c-format
msgid "&lt;partitioning config:type=\"list\"&gt;\n"
      "  &lt;drive&gt;\n"
      "    &lt;device&gt;/dev/sda&lt;/device&gt;\n"
      "    &lt;type config:type=\"symbol\"&gt;CT_DISK&lt;/type&gt;\n"
      "    &lt;use&gt;all&lt;/use&gt;\n"
      "    &lt;enable_snapshots config:type=\"boolean\"&gt;true&lt;/enable_snapshots&gt;\n"
      "    &lt;partitions config:type=\"list\"&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;filesystem config:type=\"symbol\"&gt;btrfs&lt;/filesystem&gt;\n"
      "        &lt;mount&gt;/&lt;/mount&gt;\n"
      "        &lt;create config:type=\"boolean\"&gt;true&lt;/create&gt;\n"
      "        &lt;size&gt;max&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;filesystem config:type=\"symbol\"&gt;swap&lt;/filesystem&gt;\n"
      "        &lt;mount&gt;swap&lt;/mount&gt;\n"
      "        &lt;create config:type=\"boolean\"&gt;true&lt;/create&gt;\n"
      "        &lt;size&gt;2GiB&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "  &lt;/drive&gt;\n"
      "\n"
      "  &lt;drive&gt;\n"
      "    &lt;type config:type=\"symbol\"&gt;CT_DISK&lt;/type&gt;\n"
      "    &lt;device&gt;/dev/sdb&lt;/device&gt;\n"
      "    &lt;disklabel&gt;msdos&lt;/disklabel&gt;\n"
      "    &lt;use&gt;all&lt;/use&gt;\n"
      "    &lt;partitions config:type=\"list\"&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;!-- It can serve as caching device for several bcaches --&gt;\n"
      "        &lt;bcache_caching_for config:type=\"list\"&gt;\n"
      "          &lt;listentry&gt;/dev/bcache0&lt;/listentry&gt;\n"
      "        &lt;/bcache_caching_for&gt;\n"
      "        &lt;size&gt;max&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "  &lt;/drive&gt;\n"
      "\n"
      "  &lt;drive&gt;\n"
      "    &lt;type config:type=\"symbol\"&gt;CT_DISK&lt;/type&gt;\n"
      "    &lt;device&gt;/dev/sdc&lt;/device&gt;\n"
      "    &lt;use&gt;all&lt;/use&gt;\n"
      "    &lt;disklabel&gt;msdos&lt;/disklabel&gt;\n"
      "    &lt;partitions config:type=\"list\"&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;!-- It can serve as backing device for one bcache --&gt;\n"
      "        &lt;bcache_backing_for&gt;/dev/bcache0&lt;/bcache_backing_for&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "  &lt;/drive&gt;\n"
      "\n"
      "  &lt;drive&gt;\n"
      "    &lt;type config:type=\"symbol\"&gt;CT_BCACHE&lt;/type&gt;\n"
      "    &lt;device&gt;/dev/bcache0&lt;/device&gt;\n"
      "    &lt;bcache_options&gt;\n"
      "      &lt;cache_mode&gt;writethrough&lt;/cache_mode&gt;\n"
      "    &lt;/bcache_options&gt;\n"
      "    &lt;use&gt;all&lt;/use&gt;\n"
      "    &lt;partitions config:type=\"list\"&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;mount&gt;/data&lt;/mount&gt;\n"
      "        &lt;size&gt;20GiB&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;mount&gt;swap&lt;/mount&gt;\n"
      "        &lt;filesystem config:type=\"symbol\"&gt;swap&lt;/filesystem&gt;\n"
      "        &lt;size&gt;1GiB&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "  &lt;/drive&gt;\n"
      "&lt;/partitioning&gt;\n"
      ""
msgstr ""

#. Tag: para
