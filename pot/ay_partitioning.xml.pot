#. Tag: term
#: ay_partitioning.xml:782
#, no-c-format
msgid "partition_type"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:784
#, no-c-format
msgid "Optional. Allowed values are <literal>primary</literal> (default) and <literal>logical</literal> .When using an <literal>msdos</literal> partition table, this element sets the type of the partition. The value can be <literal>primary</literal> or <literal>logical</literal> . This value is ignored when using a <literal>gpt</literal> partition table, because such a distinction does not exist in that case."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:792
#, no-c-format
msgid "&lt;partition_type&gt;primary&lt;/partition_type&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:796
#, no-c-format
msgid "mountby"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:798
#, no-c-format
msgid "Instead of a partition number, you can tell &ay; to mount a partition by <literal>device</literal> , <literal>label</literal> , <literal>uuid</literal> , <literal>path</literal> or <literal>id</literal> , which are the udev path and udev id (see <filename>/dev/disk/...</filename> )."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:805
#, no-c-format
msgid "See <literal>label</literal> and <literal>uuid</literal> documentation above. The default depends on &yast; and usually is <literal>id</literal> ."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:810
#, no-c-format
msgid "&lt;mountby config:type=\"symbol\"&gt;label&lt;/mountby&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:814
#, no-c-format
msgid "subvolumes"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:816
#, no-c-format
msgid "List of subvolumes to create for a file system of type Btrfs. This key only makes sense for file systems of type Btrfs. (See <xref linkend=\"ay-btrfs-subvolumes\"/> for more information.)"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:821
#, no-c-format
msgid "If no <tag>subvolumes</tag> section has been defined for a partition description, &ay; will create a predefined set of subvolumes for the given mount point."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:826
#, no-c-format
msgid "&lt;subvolumes config:type=\"list\"&gt;\n"
      "  &lt;path&gt;tmp&lt;/path&gt;\n"
      "  &lt;path&gt;opt&lt;/path&gt;\n"
      "  &lt;path&gt;srv&lt;/path&gt;\n"
      "  &lt;path&gt;var&lt;/path&gt;\n"
      "  ...\n"
      "&lt;/subvolumes&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:836
#, no-c-format
msgid "create_subvolumes"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:838
#, no-c-format
msgid "Determine whether Btrfs subvolumes should be created or not. It is set to <literal>true</literal> by default. When set to <literal>false</literal> , no subvolumes will be created."
msgstr ""

#. Tag: term
#: ay_partitioning.xml:846
#, no-c-format
msgid "subvolumes_prefix"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:848
#, no-c-format
msgid "Set the Btrfs subvolumes prefix name. If no prefix is wanted, it must be set to an empty value:"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:853
#, no-c-format
msgid "It is set to <literal>@</literal> by default."
msgstr ""

#. Tag: term
#: ay_partitioning.xml:859
#, no-c-format
msgid "lv_name"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:861
#, no-c-format
msgid "If this partition is on a logical volume in a volume group, specify the logical volume name here (see the <literal>type</literal> parameter in the drive configuration)."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:866
#, no-c-format
msgid "&lt;lv_name&gt;opt_lv&lt;/lv_name&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:870
#, no-c-format
msgid "stripes"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:872
#, no-c-format
msgid "An integer that configures LVM striping. Specify across how many devices you want to stripe (spread data)."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:876
#, no-c-format
msgid "&lt;stripes config:type=\"integer\"&gt;2&lt;/stripes&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:880
#, no-c-format
msgid "stripesize"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:882
#, no-c-format
msgid "Specify the size of each block in KB."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:885
#, no-c-format
msgid "&lt;stripesize config:type=\"integer\"&gt;4&lt;/stripesize&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:889
#, no-c-format
msgid "lvm_group"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:891
#, no-c-format
msgid "If this is a physical partition used by (part of) a volume group (LVM), you need to specify the name of the volume group here."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:895
#, no-c-format
msgid "&lt;lvm_group&gt;system&lt;/lvm_group&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:899
#, no-c-format
msgid "pool"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:901
#, no-c-format
msgid "<literal>pool</literal> must be set to <literal>true</literal> if the LVM logical volume should be an LVM thin pool."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:905
#, no-c-format
msgid "&lt;pool config:type=\"boolean\"&gt;true&lt;/pool&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:909
#, no-c-format
msgid "used_pool"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:911
#, no-c-format
msgid "The name of the LVM thin pool that is used as a data store for this thin logical volume. If this is set to something non-empty, it implies that the volume is a so-called thin logical volume."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:916
#, no-c-format
msgid "&lt;used_pool&gt;my_thin_pool&lt;/used_pool&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:920
#, no-c-format
msgid "raid_name"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:922
#, no-c-format
msgid "If this physical volume is part of a RAID array, specify the name of the RAID array."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:926
#, no-c-format
msgid "&lt;raid_name&gt;/dev/md/0&lt;/raid_name&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:930
#, no-c-format
msgid "raid_options"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:932
#, no-c-format
msgid "Specify RAID options. Setting the RAID options at the <literal>partition</literal> level is deprecated. See <xref linkend=\"ay-partition-sw-raid\"/> ."
msgstr ""

#. Tag: term
#: ay_partitioning.xml:940
#, no-c-format
msgid "bcache_backing_for"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:942
#, no-c-format
msgid "If this device is used as a <emphasis>&bcache; backing device</emphasis> , specify the name of the &bcache; device. See <xref linkend=\"ay-partition-bcache\"/> for further details."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:947
#, no-c-format
msgid "&lt;bcache_backing_for&gt;/dev/bcache0&lt;/bcache_backing_for&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:951
#, no-c-format
msgid "bcache_caching_for"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:953
#, no-c-format
msgid "If this device is used as a <emphasis>&bcache; caching device</emphasis> , specify the names of the &bcache; devices. See <xref linkend=\"ay-partition-bcache\"/> for further details."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:958
#, no-c-format
msgid "&lt;bcache_caching_for config:type=\"list\"&gt;&lt;listentry&gt;/dev/bcache0&lt;/listentry&gt;&lt;/bcache_caching_for&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:962
#, no-c-format
msgid "resize"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:964
#, no-c-format
msgid "Starting with &productname; 15 resizing works with physical disk partitions and with LVM volumes"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:968
#, no-c-format
msgid "Resizing works with physical disk partitions and with LVM volumes."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:971
#, no-c-format
msgid "&lt;resize config:type=\"boolean\"&gt;false&lt;/resize&gt;"
msgstr ""

#. Tag: title
#: ay_partitioning.xml:977
#, no-c-format
msgid "Btrfs subvolumes"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:978
#, no-c-format
msgid "As mentioned in <xref linkend=\"ay-partition-configuration\"/> , it is possible to define a set of subvolumes for each Btrfs file system. In its simplest form, they are specified using a list of paths:"
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:983
#, no-c-format
msgid "&lt;subvolumes config:type=\"list\"&gt;\n"
      "  &lt;path&gt;usr/local&lt;/path&gt;\n"
      "  &lt;path&gt;tmp&lt;/path&gt;\n"
      "  &lt;path&gt;opt&lt;/path&gt;\n"
      "  &lt;path&gt;srv&lt;/path&gt;\n"
      "  &lt;path&gt;var&lt;/path&gt;\n"
      "&lt;/subvolumes&gt;"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:990
#, no-c-format
msgid "However, it is possible to specify additional settings for each subvolume. For example, we might want to set a quota or to disable the copy-on-write mechanism. For that purpose, it is possible to expand any of the elements of the list as shown in the example below:"
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:996
#, no-c-format
msgid "&lt;subvolumes config:type=\"list\"&gt;\n"
      "  &lt;listentry&gt;usr/local&lt;/listentry&gt;\n"
      "  &lt;listentry&gt;\n"
      "    &lt;path&gt;tmp&lt;/path&gt;\n"
      "    &lt;referenced_limit&gt;1 GiB&lt;/referenced_limit&gt;\n"
      "  &lt;/listentry&gt;\n"
      "  &lt;listentry&gt;opt&lt;/listentry&gt;\n"
      "  &lt;listentry&gt;srv&lt;/listentry&gt;\n"
      "  &lt;listentry&gt;\n"
      "    &lt;path&gt;var/lib/pgsql&lt;/path&gt;\n"
      "    &lt;copy_on_write config:type=\"boolean\"&gt;false&lt;/copy_on_write&gt;\n"
      "  &lt;/listentry&gt;\n"
      "&lt;/subvolumes&gt;"
msgstr ""

#. Tag: term
#: ay_partitioning.xml:1012
#, no-c-format
msgid "<literal>path</literal>"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1014
#, no-c-format
msgid "Mount point for the subvolume."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1017
#, no-c-format
msgid "&lt;path&gt;tmp&lt;/tmp&gt;"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1018
#, no-c-format
msgid "Required. &ay; will ignore the subvolume if the <literal>path</literal> is not specified."
msgstr ""

#. Tag: term
#: ay_partitioning.xml:1025
#, no-c-format
msgid "<literal>copy-on-write</literal>"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1027
#, no-c-format
msgid "Whether copy-on-write should be enabled for the subvolume."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1030
#, no-c-format
msgid "&lt;copy-on-write config:type=\"boolean\"&gt;false&lt;/copy-on-write&gt;"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1031
#, no-c-format
msgid "Optional. The default value is <literal>false</literal> ."
msgstr ""

#. Tag: term
#: ay_partitioning.xml:1037
#, no-c-format
msgid "<literal>referenced_limit</literal>"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1039
#, no-c-format
msgid "Set a quota for the subvolume."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1042
#, no-c-format
msgid "&lt;referenced_limit&gt;1 GiB&lt;/referenced_limit&gt;"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1043
#, no-c-format
msgid "Optional. The default value is <literal>unlimited</literal> . Btrfs supports two kinds of limits: <literal>referenced</literal> and <literal>exclusive</literal> . At this point, only the former is supported."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1052
#, no-c-format
msgid "If there is a default subvolume used for the distribution (for example <filename>@</filename> in &productname;), the name of this default subvolume is automatically prefixed to the names of the defined subvolumes. This behavior can be disabled by setting the <literal>subvolumes_prefix</literal> in the <xref linkend=\"CreateProfile-Partitioning-config\"/> section."
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1060
#, no-c-format
msgid "&lt;subvolumes_prefix&gt;&lt;![CDATA[]]&gt;&lt;/subvolumes_prefix&gt;"
msgstr ""

#. Tag: title
#: ay_partitioning.xml:1063
#, no-c-format
msgid "Using the whole disk"
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1064
#, no-c-format
msgid "&ay; allows to use a whole disk without creating any partition by setting the <literal>disklabel</literal> to <literal>none</literal> as described in <xref linkend=\"CreateProfile-Partitioning-config\"/> . In such cases, the configuration in the first <literal>partition</literal> from the <literal>drive</literal> will be applied to the whole disk."
msgstr ""

#. Tag: para
#: ay_partitioning.xml:1071
#, no-c-format
msgid "In the example below, we are using the second disk ( <literal>/dev/sdb</literal> ) as the <literal>/home</literal> file system."
msgstr ""

#. Tag: title
#: ay_partitioning.xml:1076
#, no-c-format
msgid "Using a whole disk as a file system"
msgstr ""

#. Tag: screen
#: ay_partitioning.xml:1077
#, no-c-format
msgid "&lt;partitioning config:type=\"list\"&gt;\n"
      "  &lt;drive&gt;\n"
      "    &lt;device&gt;/dev/sda&lt;/device&gt;\n"
      "    &lt;partitions config:type=\"list\"&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;create config:type=\"boolean\"&gt;true&lt;/create&gt;\n"
      "        &lt;format config:type=\"boolean\"&gt;true&lt;/format&gt;\n"
      "        &lt;mount&gt;/&lt;/mount&gt;\n"
      "        &lt;size&gt;max&lt;/size&gt;\n"
      "      &lt;/partition&gt;\n"
      "    &lt;/partitions&gt;\n"
      "  &lt;/drive&gt;\n"
      "  &lt;drive&gt;\n"
      "    &lt;device&gt;/dev/sdb&lt;/device&gt;\n"
      "    &lt;disklabel&gt;none&lt;/disklabel&gt;\n"
      "    &lt;partitions config:type=\"list\"&gt;\n"
      "      &lt;partition&gt;\n"
      "        &lt;format config:type=\"boolean\"&gt;true&lt;/format&gt;\n"
