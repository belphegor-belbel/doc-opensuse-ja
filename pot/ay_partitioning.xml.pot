      "  &lt;zfcp&gt;\n"
      "    &lt;devices config:type=\"list\"&gt;\n"
      "      &lt;listentry&gt;\n"
      "        ...\n"
      "      &lt;/listentry&gt;\n"
      "    &lt;/devices&gt;\n"
      "  &lt;/zfcp&gt;\n"
      "&lt;profile&gt;"
msgstr ""

