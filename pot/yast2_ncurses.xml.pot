#. Tag: title
#: yast2_ncurses.xml:498
#, no-c-format
msgid "yast dhcp-server"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:499
#, no-c-format
msgid "Manages the DHCP server and configures its settings. <command>yast dhcp-server</command> accepts the following commands:"
msgstr ""

#. Tag: term
#: yast2_ncurses.xml:505 yast2_ncurses.xml:757 yast2_ncurses.xml:1527
#, no-c-format
msgid "disable"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:507
#, no-c-format
msgid "Disables the DHCP server service."
msgstr ""

#. Tag: term
#: yast2_ncurses.xml:513 yast2_ncurses.xml:765 yast2_ncurses.xml:1536
#, no-c-format
msgid "enable"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:515
#, no-c-format
msgid "Enables the DHCP server service."
msgstr ""

#. Tag: term
#: yast2_ncurses.xml:521 yast2_ncurses.xml:612
#, no-c-format
msgid "host"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:523
#, no-c-format
msgid "Configures settings for individual hosts."
msgstr ""

#. Tag: term
#: yast2_ncurses.xml:529
#, no-c-format
msgid "interface"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:531
#, no-c-format
msgid "Specifies to which network interface to listen to:"
msgstr ""

#. Tag: screen
#: yast2_ncurses.xml:534
#, no-c-format
msgid ""
      "&prompt.sudo;yast dhcp-server interface current\n"
      "Selected Interfaces: eth0\n"
      "Other Interfaces: bond0, pbu, eth1"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:535
#, no-c-format
msgid "For a complete list of options, run <command>yast dhcp-server interface help</command>."
msgstr ""

#. Tag: term
#: yast2_ncurses.xml:542
#, no-c-format
msgid "options"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:544
#, no-c-format
msgid "Manages global DHCP options. For a complete list of options, run <command>yast dhcp-server options help</command>."
msgstr ""

#. Tag: term
#: yast2_ncurses.xml:551 yast2_ncurses.xml:2049
#, no-c-format
msgid "status"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:553
#, no-c-format
msgid "Prints the status of the DHCP service."
msgstr ""

#. Tag: term
#: yast2_ncurses.xml:559
