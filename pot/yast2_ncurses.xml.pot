msgstr ""

#. Tag: keycombo
#: yast2_ncurses.xml:238
#, no-c-format
msgid "<keycap function=\"control\"></keycap><keycap>L</keycap>"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:241
#, no-c-format
