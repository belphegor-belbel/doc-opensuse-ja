#. Tag: title
#: common_intro_feedback_i.xml:9
#, no-c-format
msgid "Improving the documentation"
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:17
#, no-c-format
msgid "Your feedback and contributions to this documentation are welcome. The following channels for giving feedback are available:"
msgstr ""

#. Tag: term
#: common_intro_feedback_i.xml:24
#, no-c-format
msgid "Service requests and support"
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:26
#, no-c-format
msgid "For services and support options available for your product, see <link xlink:href=\"https://www.suse.com/support/\"/> ."
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:30
#, no-c-format
msgid "To open a service request, you need a &suse; subscription registered at &scc;. Go to <link xlink:href=\"https://scc.suse.com/support/requests\"/> , log in, and click <guimenu>Create New</guimenu> ."
msgstr ""

#. Tag: term
#: common_intro_feedback_i.xml:39
#, no-c-format
msgid "Bug reports"
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:41
#, no-c-format
msgid "Report issues with the documentation at <link os=\"sles;sled;slemicro\" xlink:href=\"https://bugzilla.suse.com/\"/> <link os=\"osuse\" xlink:href=\"https://bugzilla.opensuse.org/\"/> ."
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:47
#, no-c-format
msgid "To simplify this process, click the <guimenu>Report an issue</guimenu> icon next to a headline in the HTML version of this document. This preselects the right product and category in Bugzilla and adds a link to the current section. You can start typing your bug report right away."
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:54
#, no-c-format
msgid "A Bugzilla account is required."
msgstr ""

#. Tag: term
#: common_intro_feedback_i.xml:60
#, no-c-format
msgid "Contributions"
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:62
#, no-c-format
msgid "To contribute to this documentation, click the <guimenu>Edit source document</guimenu> icon next to a headline in the HTML version of this document. This will take you to the source code on GitHub, where you can open a pull request."
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:67
#, no-c-format
msgid "A GitHub account is required."
msgstr ""

#. Tag: title
#: common_intro_feedback_i.xml:71
#, no-c-format
msgid "<guimenu>Edit source document</guimenu> only available for English"
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:72
#, no-c-format
msgid "The <guimenu>Edit source document</guimenu> icons are only available for the English version of each document. For all other languages, use the <guimenu>Report an issue</guimenu> icons instead."
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:78
#, no-c-format
msgid "For more information about the documentation environment used for this documentation, see the repository's README at <link xlink:href=\"&github.url;/blob/main/README.adoc\"/> <!--taroth 2020-01-09: Link needs to be adjusted in case the README changes format. In case the READMEs differ between the branches, better just point to &github.url;-->"
msgstr ""

#. Tag: term
#: common_intro_feedback_i.xml:91
#, no-c-format
msgid "Mail"
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:93
#, no-c-format
msgid "You can also report errors and send feedback concerning the documentation to <email>doc-team@suse.com</email> . Include the document title, the product version, and the publication date of the document. Additionally, include the relevant section number and title (or provide the URL), and provide a concise description of the problem."
msgstr ""

#. Tag: term
#: common_intro_feedback_i.xml:103
#, no-c-format
msgid "Help"
msgstr ""

#. Tag: para
#: common_intro_feedback_i.xml:105
#, no-c-format
msgid "If you need further help on &productname;, see <link xlink:href=\"https://en.opensuse.org/Portal:Support\"/> ."
msgstr ""

