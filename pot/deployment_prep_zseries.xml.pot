#. Tag: title
#: deployment_prep_zseries.xml:390
#, no-c-format
msgid "Specifying the installation source and &yast; interface"
msgstr ""

#. Tag: term
#: deployment_prep_zseries.xml:393
#, no-c-format
msgid "<command>Install=nfs://server/directory/DVD1/</command>"
msgstr ""

#. Tag: para
#: deployment_prep_zseries.xml:395
#, no-c-format
msgid "Specify the location of the installation source to use. Supported protocols are <literal>nfs</literal> , <literal>smb</literal> (Samba/CIFS), <literal>ftp</literal> , <literal>tftp</literal> <literal>http</literal> , and <literal>https</literal> ."
msgstr ""

#. Tag: para
#: deployment_prep_zseries.xml:401
#, no-c-format
msgid "If an <literal>ftp</literal> , <literal>tftp</literal> or <literal>smb</literal> URL is provided, specify the user name and password. Skip credentials for anonymous or guest login."
msgstr ""

#. Tag: screen
#: deployment_prep_zseries.xml:406
#, no-c-format
msgid "Install=ftp://<replaceable>USER</replaceable>:<replaceable>PASSWORD</replaceable>@<replaceable>SERVER</replaceable>/<replaceable>DIRECTORY</replaceable>/DVD1/\n"
      "Install=tftp://<replaceable>USER</replaceable>:<replaceable>PASSWORD</replaceable>@<replaceable>SERVER</replaceable>/<replaceable>DIRECTORY</replaceable>/DVD1/"
msgstr ""

#. Tag: para
#: deployment_prep_zseries.xml:408
#, no-c-format
msgid "If you want to perform the installation over an encrypted connection, use an <literal>https</literal> URL. If the certificate cannot be verified, use the <literal>sslcerts=0</literal> boot option to disable certificate checking."
msgstr ""

#. Tag: para
#: deployment_prep_zseries.xml:414
#, no-c-format
msgid "In case of a Samba or CIFS installation, you can also specify the domain:"
msgstr ""

#. Tag: screen
#: deployment_prep_zseries.xml:418
#, no-c-format
msgid "Install=smb://<replaceable>WORKDOMAIN</replaceable>;<replaceable>USER</replaceable>:<replaceable>PASSWORD</replaceable>@<replaceable>SERVER</replaceable>/<replaceable>DIRECTORY</replaceable>/DVD1/"
msgstr ""

#. Tag: term
#: deployment_prep_zseries.xml:422
#, no-c-format
msgid "<command>ssh=1</command> &nbsp; <command>vnc=1</command> &nbsp; <command>Display_IP=192.168.42.42</command>"
msgstr ""

#. Tag: para
#: deployment_prep_zseries.xml:424
#, no-c-format
msgid "The installation method depends on which parameter you specify. <envar>ssh</envar> enables SSH installation, <envar>vnc</envar> starts a VNC server on the installing machine, and <envar>Display_IP</envar> causes the installing system to try to connect to an X server at the specified address. Only one of these parameters should be set."
msgstr ""

#. Tag: title
#: deployment_prep_zseries.xml:432
#, no-c-format
msgid "X authentication mechanism"
msgstr ""

#. Tag: para
#: deployment_prep_zseries.xml:433
#, no-c-format
msgid "The direct installation with the X Window System relies on an authentication mechanism based on host names. This mechanism is disabled on current &productname; versions. We recommend to perform an installation using SSH or VNC is preferred."
