#. Tag: screen
#: deployment_prep_zseries.xml:454
#, no-c-format
msgid "ssh.password=&lt;a password&gt;"
msgstr ""

#. Tag: title
#: deployment_prep_zseries.xml:461
#, no-c-format
msgid "Example parmfiles"
msgstr ""

#. Tag: para
#: deployment_prep_zseries.xml:462
#, no-c-format
msgid "The maximum capacity of a parmfile is 860 characters. As a rule of thumb, the parmfile should contain a maximum of 10 lines with no more than 79 characters. When reading a parmfile, all lines are concatenated without adding white spaces, therefore the last character (79) of each line needs to be a <keycap function=\"space\"/> ."
msgstr ""

#. Tag: para
#: deployment_prep_zseries.xml:469
#, no-c-format
msgid "To receive potential error messages on the console, use"
msgstr ""

#. Tag: screen
#: deployment_prep_zseries.xml:472
#, no-c-format
msgid "linuxrclog=/dev/console"
msgstr ""

