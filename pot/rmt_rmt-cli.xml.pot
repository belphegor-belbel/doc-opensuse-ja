#, no-c-format
msgid "Mirror repositories."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:67
#, no-c-format
msgid "<command>rmt-cli systems</command>"
msgstr ""

#. Tag: para
#: rmt_rmt-cli.xml:69
#, no-c-format
msgid "List and modify systems."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:75
#, no-c-format
msgid "<command>rmt-cli import</command>"
msgstr ""

#. Tag: para
#: rmt_rmt-cli.xml:77
#, no-c-format
msgid "Import commands for offline mode."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:83
#, no-c-format
msgid "<command>rmt-cli export</command>"
msgstr ""

#. Tag: para
#: rmt_rmt-cli.xml:85
#, no-c-format
msgid "Export commands for offline mode."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:91
#, no-c-format
