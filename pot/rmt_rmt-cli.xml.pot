#, no-c-format
msgid "List and modify repositories."
msgstr ""

#. Tag: para
#: rmt_rmt-cli.xml:54
#, no-c-format
msgid "Mirror repositories."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:60
#, no-c-format
msgid "<command>rmt-cli systems</command>"
msgstr ""

#. Tag: para
#: rmt_rmt-cli.xml:62
#, no-c-format
msgid "List and modify systems."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:68
#, no-c-format
msgid "<command>rmt-cli import</command>"
msgstr ""

#. Tag: para
#: rmt_rmt-cli.xml:70
#, no-c-format
msgid "Import commands for offline mode."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:76
#, no-c-format
msgid "<command>rmt-cli export</command>"
msgstr ""

#. Tag: para
#: rmt_rmt-cli.xml:78
#, no-c-format
msgid "Export commands for offline mode."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:84
#, no-c-format
