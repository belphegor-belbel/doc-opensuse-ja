#: rmt_rmt-cli.xml:28
#, no-c-format
msgid "<command>rmt-cli sync</command>"
msgstr ""

#. Tag: para
#: rmt_rmt-cli.xml:30
#, no-c-format
msgid "Synchronize database with &scc;."
msgstr ""

#. Tag: term
#: rmt_rmt-cli.xml:36
