#. Tag: term
#: ay_networking.xml:515
#, no-c-format
msgid "<literal>bonding_module_opts</literal>"
msgstr ""

#. Tag: para
#: ay_networking.xml:517
#, no-c-format
msgid "Options for bonding device."
msgstr ""

#. Tag: para
#: ay_networking.xml:520
#, no-c-format
msgid "Used only with <literal>bond</literal> device."
msgstr ""

#. Tag: term
#: ay_networking.xml:526
#, no-c-format
msgid "<literal>mtu</literal>"
msgstr ""

#. Tag: para
#: ay_networking.xml:528
#, no-c-format
msgid "Maximum transmission unit for the interface."
msgstr ""

#. Tag: term
#: ay_networking.xml:537
#, no-c-format
msgid "<literal>ethtool_options</literal>"
msgstr ""

#. Tag: para
#: ay_networking.xml:539
#, no-c-format
msgid "Ethtool options during device activation."
msgstr ""

#. Tag: term
#: ay_networking.xml:548
#, no-c-format
msgid "<literal>zone</literal>"
msgstr ""

#. Tag: para
#: ay_networking.xml:550
#, no-c-format
msgid "Firewall zone name which the interface is assigned to."
msgstr ""

#. Tag: term
#: ay_networking.xml:559
#, no-c-format
msgid "<literal>vlan_id</literal>"
msgstr ""

#. Tag: para
#: ay_networking.xml:561
#, no-c-format
msgid "Identifier used for this VLAN."
msgstr ""

#. Tag: para
#: ay_networking.xml:564
#, no-c-format
msgid "Used only with a <literal>vlan</literal> device."
msgstr ""

#. Tag: term
#: ay_networking.xml:570
#, no-c-format
msgid "<literal>etherdevice</literal>"
msgstr ""

#. Tag: para
#: ay_networking.xml:572
#, no-c-format
msgid "Device to which VLAN is attached."
msgstr ""

#. Tag: para
#: ay_networking.xml:575
#, no-c-format
msgid "Used only with a <literal>vlan</literal> device and required for it."
msgstr ""

#. Tag: term
#: ay_networking.xml:581
#, no-c-format
msgid "<literal>bridge</literal>"
msgstr ""

#. Tag: para
#: ay_networking.xml:583
#, no-c-format
msgid "<literal>yes</literal> if interface is a bridge."
msgstr ""

#. Tag: para
#: ay_networking.xml:586
#, no-c-format
msgid "Deprecated. It is inferred from other attributes."
msgstr ""

#. Tag: term
#: ay_networking.xml:592
#, no-c-format
msgid "<literal>bridge_ports</literal>"
msgstr ""

#. Tag: para
#: ay_networking.xml:594
#, no-c-format
msgid "Space-separated list of bridge ports, for example, <literal>eth0 eth1</literal> ."
msgstr ""

#. Tag: para
#: ay_networking.xml:598
#, no-c-format
msgid "Used only with a <literal>bridge</literal> device and required for it."
msgstr ""

