#. Tag: title
#: security_ldap_users.xml:13
#, no-c-format
msgid "Managing LDAP users and groups"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:15
#, no-c-format
msgid "Users and groups are created and managed with the <command>dsidm</command> command."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:20
#, no-c-format
msgid "The following examples use the instance name <replaceable>LDAP1</replaceable> . Replace this with your instance name."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:25
#, no-c-format
msgid "List your existing users and groups:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:29
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> user list</command>\n"
      "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> group list</command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:32
#, no-c-format
msgid "List all information on a single user:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:36
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> user get <replaceable>USER</replaceable></command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:38
#, no-c-format
msgid "List all information on a single group:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:42
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> group get <replaceable>GROUP</replaceable></command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:44
#, no-c-format
msgid "List members of a group:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:48
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> group members <replaceable>GROUP</replaceable></command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:50
#, no-c-format
msgid "In the following example we add one user, &exampleuserII;. The example server instance is named <replaceable>LDAP1</replaceable> , and the instance's suffix is dc= <replaceable>LDAP1</replaceable> ,dc= <replaceable>COM</replaceable> ."
msgstr ""

#. Tag: title
#: security_ldap_users.xml:58
#, no-c-format
msgid "Creating LDAP users"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:59
#, no-c-format
msgid "The following example creates the user &exampleuserIIfull; on your &ds389a; instance:"
msgstr ""

#. Tag: step
#: security_ldap_users.xml:63
#, no-c-format
msgid " <screen>&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> user create --uid &exampleuserII_plain; \\</command>\n"
      " <command>--cn &exampleuserII_plain; --displayName '&exampleuserIIfull;' --uidNumber 1001 --gidNumber 101 \\</command>\n"
      " <command> --homeDirectory /home/&exampleuserII_plain;</command></screen> "
msgstr ""

#. Tag: para
#: security_ldap_users.xml:69
#, no-c-format
msgid "Verify by looking up your new user's <literal>distinguished name</literal> (fully qualified name to the directory object, which is guaranteed unique):"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:73
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> user get &exampleuserII_plain;</command>\n"
      "dn: uid=&exampleuserII_plain;,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable>\n"
      "[...]"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:76
#, no-c-format
msgid "You need the distinguished name for actions such as changing the password for a user."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:82
#, no-c-format
msgid "Create a password for new user &exampleuserII;:"
msgstr ""

#. Tag: step
#: security_ldap_users.xml:86
#, no-c-format
msgid " <screen>&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> account reset_password \\</command>\n"
      " <command>uid=&exampleuserII_plain;,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable></command></screen> "
msgstr ""

#. Tag: para
#: security_ldap_users.xml:91
#, no-c-format
msgid "Enter the new password for &exampleuserII; twice."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:94
#, no-c-format
msgid "If the action was successful, you get the following message:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:97
#, no-c-format
msgid "\n"
      "reset password for uid=&exampleuserII_plain;,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable>\n"
      ""
msgstr ""

#. Tag: para
#: security_ldap_users.xml:100
#, no-c-format
msgid "Use the same command to change an existing password."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:107
#, no-c-format
msgid "Verify that the user's password works:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:110
#, no-c-format
msgid "\n"
      "&prompt.user;<command>ldapwhoami -D uid=&exampleuserII_plain;,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable> -W</command>\n"
      "Enter LDAP Password: <replaceable>PASSWORD</replaceable>\n"
      "dn: uid=&exampleuserII_plain;,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable>\n"
      "     "
msgstr ""

#. Tag: para
#: security_ldap_users.xml:118
#, no-c-format
msgid "The following example deletes the user &exampleuserII;:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:122
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> user delete</command>\n"
      "   Enter dn to delete : uid=wilber,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable>\n"
      "   Deleting nsUserAccount uid=wilber,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable> :\n"
      "   Type 'Yes I am sure' to continue: <command>Yes I am sure</command>\n"
      "   Successfully deleted uid=wilber,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable>"
msgstr ""

#. Tag: title
#: security_ldap_users.xml:129
#, no-c-format
msgid "Creating LDAP groups and assigning users to them"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:130
#, no-c-format
msgid "In the following examples, we create a group, <replaceable>server_admins</replaceable> , and assign the user &exampleuserII; to this group. The example server instance is named <replaceable>LDAP1</replaceable> , and the instance's suffix is dc= <replaceable>LDAP1</replaceable> ,dc= <replaceable>COM</replaceable> ."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:138
#, no-c-format
msgid "Create the group:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:141
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> group create</command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:142
#, no-c-format
msgid "You will be prompted for a group name. Enter your chosen group name, which in the following example is <replaceable>SERVER_ADMINS</replaceable> :"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:146
#, no-c-format
msgid "Enter value for cn : <replaceable>SERVER_ADMINS</replaceable>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:149
#, no-c-format
msgid "Add the user &exampleuserII; to the group:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:152
#, no-c-format
msgid "\n"
      "&prompt.sudo;<command>dsidm <replaceable>LDAP1</replaceable> group add_member <replaceable>SERVER_ADMINS</replaceable></command> \\\n"
      "<command>uid=&exampleuserII_plain;,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable></command>\n"
      "added member: uid=&exampleuserII_plain;,ou=people,dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable>"
msgstr ""

