#. Tag: title
#: security_ldap_users.xml:9
#, no-c-format
msgid "Managing LDAP users and groups"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:10
#, no-c-format
msgid "Users and groups are created and managed with the <command>dsidm</command> command. It runs interactively, or you can run it on the command line, and enter all options in a single command."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:15
#, no-c-format
msgid "The following examples use the instance name <replaceable>ldap1</replaceable> . Replace this with your instance name."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:19
#, no-c-format
msgid "List your existing users and groups:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:22
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> user list</command>\n"
      "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> group list</command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:24
#, no-c-format
msgid "List all information on a single user:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:27
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> user get <replaceable>username</replaceable></command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:28
#, no-c-format
msgid "List all information on a single group:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:31
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> group get <replaceable>groupname</replaceable></command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:32
#, no-c-format
msgid "List members of a group:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:35
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> group members <replaceable>demo_group</replaceable></command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:36
#, no-c-format
msgid "In the following example we add two users, &exampleuserII; and &exampleuserIII;, by specifying their data via command-line arguments. The example server instance is named <replaceable>ldap1</replaceable> , and the instance's suffix is <replaceable>dc=ldap1,dc=com</replaceable> ."
msgstr ""

#. Tag: title
#: security_ldap_users.xml:43
#, no-c-format
msgid "Creating LDAP users"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:44
#, no-c-format
msgid "LDAP users are your users that already exist. They should have Linux system accounts, with Linux logins and home directories. Adding them to your LDAP server provides central user management for your network. You need to enter accurate user data, which you can obtain by accessing the computers that hold their accounts, and running the <command>id</command> command, like this example for &exampleuserIIfull;:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:52
#, no-c-format
msgid "&prompt.user;<command>id &exampleuserII_plain;</command>\n"
      "uid=1001(wilber) gid=101(users) groups=101(users)"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:55
#, no-c-format
msgid "Create the LDAP user &exampleuserII;:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:58
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> user create --uid &exampleuserII_plain; \\</command>\n"
      "  <command>--cn &exampleuserII_plain; --displayName '&exampleuserIIfull;' --uidNumber 1001 --gidNumber 101 \\</command>\n"
      " <command> --homeDirectory /home/&exampleuserII_plain;</command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:63
#, no-c-format
msgid "Verify by looking up your new user's <literal>distinguished name</literal> (fully qualified name to the directory object, which is guaranteed unique):"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:68
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> user get &exampleuserII_plain;</command>\n"
      "dn: uid=&exampleuserII_plain;,ou=people,dc=ldap1,dc=com\n"
      "[...]"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:71
#, no-c-format
msgid "You need the distinguished name for actions such as changing the password for a user."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:77
#, no-c-format
msgid "Create a password for new user &exampleuserII;:"
msgstr ""

#. Tag: step
#: security_ldap_users.xml:81
#, no-c-format
msgid " <screen>&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> account reset_password \\</command>\n"
      " <command>uid=&exampleuserII_plain;,ou=people,dc=<replaceable>ldap1,dc=com</replaceable></command></screen> "
msgstr ""

#. Tag: para
#: security_ldap_users.xml:86
#, no-c-format
msgid "Enter the new password for &exampleuserII; twice."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:89
#, no-c-format
msgid "If the action was successful, you get the following message:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:92
#, no-c-format
msgid "reset password for uid=&exampleuserII_plain;,ou=people,dc=ldap1,dc=com"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:93
#, no-c-format
msgid "Use the same command to change an existing password."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:100
#, no-c-format
msgid "Verify that the user's password works:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:103
#, no-c-format
msgid "&prompt.user;<command>ldapwhoami -D uid=&exampleuserII_plain;,ou=people,dc=<replaceable>ldap1,dc=com</replaceable> -W</command>\n"
      "Enter LDAP Password: <replaceable>PASSWORD</replaceable>\n"
      "dn: uid=&exampleuserII_plain;,ou=people,dc=ldap1,dc=com\n"
      "     "
msgstr ""

#. Tag: para
#: security_ldap_users.xml:110
#, no-c-format
msgid "The following example deletes the user &exampleuserII;:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:113
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> user delete</command>\n"
      "   Enter dn to delete : uid=wilber,ou=people,<replaceable>dc=ldap1,dc=com</replaceable>\n"
      "   Deleting nsUserAccount uid=wilber,ou=people,dc=ldap1,dc=com :\n"
      "   Type 'Yes I am sure' to continue: <command>Yes I am sure</command>\n"
      "   Successfully deleted uid=wilber,ou=people,dc=ldap1,dc=com"
msgstr ""

#. Tag: title
#: security_ldap_users.xml:120
#, no-c-format
msgid "Creating LDAP groups and assigning users to them"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:121
#, no-c-format
msgid "In the following examples, we create a group, <replaceable>server_admins</replaceable> , and assign the user &exampleuserII; to this group. The example server instance is named <replaceable>ldap1</replaceable> , and the instance's suffix is <replaceable>dc=ldap1,dc=com</replaceable> ."
msgstr ""

#. Tag: para
#: security_ldap_users.xml:129
#, no-c-format
msgid "Create the group:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:132
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> group create</command>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:133
#, no-c-format
msgid "You will be prompted for a group name. Enter your chosen group name, which in the following example is <replaceable>server_admins</replaceable> :"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:137
#, no-c-format
msgid "Enter value for cn : <replaceable>server_admins</replaceable>"
msgstr ""

#. Tag: para
#: security_ldap_users.xml:140
#, no-c-format
msgid "Add the user &exampleuserII; to the group:"
msgstr ""

#. Tag: screen
#: security_ldap_users.xml:143
#, no-c-format
msgid "&prompt.sudo;<command>dsidm <replaceable>ldap1</replaceable> group add_member <replaceable>server_admins</replaceable> uid=&exampleuserII_plain;,ou=people,<replaceable>dc=ldap1,dc=com</replaceable></command>\n"
      "added member: uid=&exampleuserII_plain;,ou=people,dc=ldap1,dc=com"
msgstr ""

