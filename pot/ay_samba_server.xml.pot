      "          &lt;usershare_max_shares&gt;100&lt;/usershare_max_shares&gt;\n"
      "          &lt;workgroup&gt;WORKGROUP&lt;/workgroup&gt;\n"
      "        &lt;/parameters&gt;\n"
      "      &lt;/listentry&gt;\n"
      "    &lt;/config&gt;\n"
      "    &lt;service&gt;Disabled&lt;/service&gt;\n"
      "    &lt;trustdom/&gt;\n"
      "    &lt;version&gt;2.11&lt;/version&gt;\n"
