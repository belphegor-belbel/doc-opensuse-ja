msgstr ""

#. Tag: screen
#: rmt_install.xml:138
#, no-c-format
msgid "&prompt.root;zypper install rmt-server yast2-rmt nginx mariadb"
msgstr ""

#. Tag: title
#: rmt_install.xml:140
#, no-c-format
msgid "Hardware requirements"
msgstr ""

#. Tag: para
#: rmt_install.xml:141
#, no-c-format
msgid "When installing &rmt; on &minvm;, be aware that it requires a minimum of 100 GB disk space, depending on the products you select to mirror. Another requirement is a CPU with at least two cores and 2 GB of RAM."
msgstr ""

#. Tag: title
#: rmt_install.xml:150
#, no-c-format
msgid "Deploying &rmt; on top of the &kube; cluster"
msgstr ""

#. Tag: para
#: rmt_install.xml:152
#, no-c-format
msgid "This section describes how to deploy &rmt; on top of the &kube; cluster. It uses <emphasis>Helm</emphasis> as the package manager to interact with the &kube; cluster. Find more details about using Helm at <link xlink:href=\"https://helm.sh/docs/intro/using_helm/\"/> ."
msgstr ""

#. Tag: title
#: rmt_install.xml:160
#, no-c-format
msgid "Prerequisites"
msgstr ""

#. Tag: para
#: rmt_install.xml:163
#, no-c-format
msgid "Running &kube; cluster"
msgstr ""

#. Tag: para
#: rmt_install.xml:168
#, no-c-format
