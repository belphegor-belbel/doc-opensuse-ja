#. Tag: para
#: common_intro_convention.xml:92
#, no-c-format
msgid "Commands that must be run with &rootuser; privileges. You can also prefix these commands with the <command>sudo</command> command to run them as a non-privileged user:"
msgstr ""

#. Tag: screen
#: common_intro_convention.xml:97
#, no-c-format
msgid "&prompt.root;<command>command</command>\n"
      "&prompt.sudo;<command>command</command>"
msgstr ""

#. Tag: para
#: common_intro_convention.xml:101
#, no-c-format
msgid "Commands that can be run by non-privileged users:"
msgstr ""

#. Tag: screen
#: common_intro_convention.xml:104
#, no-c-format
msgid "&prompt.user;<command>command</command>"
msgstr ""

#. Tag: para
#: common_intro_convention.xml:107
#, no-c-format
msgid "Commands can be split into two or multiple lines by a backslash character ( <literal>\\</literal> ) at the end of a line. The backslash informs the shell that the command invocation will continue after the end of the line:"
msgstr ""

#. Tag: screen
#: common_intro_convention.xml:112
#, no-c-format
msgid "&prompt.user;<command>echo</command> a b \\\n"
      "c d"
msgstr ""

#. Tag: para
#: common_intro_convention.xml:116
#, no-c-format
msgid "A code block that shows both the command (preceded by a prompt) and the respective output returned by the shell:"
msgstr ""

#. Tag: screen
#: common_intro_convention.xml:120
#, no-c-format
msgid "&prompt.user;<command>command</command>\n"
      "output"
msgstr ""

