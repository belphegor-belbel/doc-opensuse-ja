msgstr ""

#. Tag: term
#: gnome_use.xml:68
#, no-c-format
msgid "Sidebar"
msgstr ""

#. Tag: para
#: gnome_use.xml:70
#, no-c-format
msgid "The sidebar lets you navigate between often-used directories and external or network storage devices. To display or hide the sidebar, press <keycap>F9</keycap> ."
msgstr ""

#. Tag: term
#: gnome_use.xml:78
#, no-c-format
msgid "Content area"
msgstr ""

#. Tag: para
#: gnome_use.xml:80
#, no-c-format
msgid "Displays files and directories."
msgstr ""

#. Tag: para
#: gnome_use.xml:83
#, no-c-format
msgid "Use the icons in the top right part of the window to switch between list and grid icon view. <!--taroth 2016-05-24: commenting because I could no longer find those options as the menu icons probably disappeared: The menu to the right of these icons can be used to further customize the view.-->"
msgstr ""

#. Tag: term
#: gnome_use.xml:93
#, no-c-format
msgid "Context menus"
msgstr ""

#. Tag: para
#: gnome_use.xml:95
#, no-c-format
