#: rpm.xml:582
#, no-c-format
msgid "<filename>SOURCES</filename>"
msgstr ""

#. Tag: para
#: rpm.xml:585
#, no-c-format
msgid "for the original sources ( <filename>.tar.bz2</filename> or <filename>.tar.gz</filename> files, etc.) and for distribution-specific adjustments (mostly <filename>.diff</filename> or <filename>.patch</filename> files)"
msgstr ""

#. Tag: term
