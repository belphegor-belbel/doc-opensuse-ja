      "      &lt;firstboot&gt;\n"
      "        &lt;firstboot_enabled config:type=\"boolean\"&gt;true&lt;/firstboot_enabled&gt;\n"
      "      &lt;/firstboot&gt;\n"
      "    "
msgstr ""

