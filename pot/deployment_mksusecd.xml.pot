#. Tag: title
#: deployment_mksusecd.xml:12
#, no-c-format
msgid "Customizing installation images with mksusecd"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:15
#, no-c-format
msgid "<command>mksusecd</command> is a useful tool for creating a customized installation image. Use it to modify the regular &sle; installation images, adding and removing files, creating a minimal network installation image, customizing boot and repository options, and creating a minimal boot image as an alternative to booting a system from a PXE server."
msgstr ""

#. Tag: title
#: deployment_mksusecd.xml:29
#, no-c-format
msgid "Installing mksusecd"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:31
#, no-c-format
msgid "In &slea;&nbsp;15, <command>mksusecd</command> is in the <literal>Development Tools</literal> module. If this module is not enabled, you must enable it first. Find the exact module name and <command>&suseconnect;</command> activation command with <command>zypper</command> :"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:38
#, no-c-format
msgid "&prompt.user;zypper search-packages mksusecd\n"
      "Following packages were found in following modules:\n"
      "\n"
      "Package              Module or Repository\n"
      "-------------------- -------------------------------------------------------------------\n"
      "---------------------- -----------------------------------------------------------------\n"
      "mksusecd             Development Tools Module (sle-module-development-tools/15.4/x86_64)\n"
      "                       SUSEConnect --product sle-module-development-tools/15.4/x86_64\n"
      "\n"
      "To activate the respective module or product, use SUSEConnect --product.\n"
      "Use SUSEConnect --help for more details."
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:50
#, no-c-format
msgid "Enable the module with &suseconnect;:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:54
#, no-c-format
msgid "&prompt.sudo;SUSEConnect --product sle-module-development-tools/15.4/x86_64"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:56
#, no-c-format
msgid "Install <command>mksusecd</command> :"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:60
#, no-c-format
msgid "&prompt.sudo;zypper in mksusecd"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:62
#, no-c-format
msgid "Run <command>mksusecd --help</command> to see a complete list of commands."
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:66
#, no-c-format
msgid "After you create your custom image, either burn it to a CD/DVD medium using your preferred disk-writing program, or create a bootable &usbflashdrive; using the <command>dd</command> command. Make sure the device is not mounted, then run the following command:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:73
#, no-c-format
msgid "&prompt.root;<command>dd</command> if=<replaceable>myinstaller.iso</replaceable> of=/dev/<replaceable>SDB</replaceable> bs=4M"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:75
#, no-c-format
msgid "Then your new bootable device is ready to use."
msgstr ""

#. Tag: title
#: deployment_mksusecd.xml:80
#, no-c-format
msgid "Creating a minimal boot image"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:82
#, no-c-format
msgid "Use <command>mksusecd</command> to create a minimal boot image to start client machines from a CD/DVD or &usbflashdrive;, in place of starting them from a PXE boot server. The minimal boot image launches the kernel and initrd, and then the remaining installation files are fetched from a local NFS server (see <xref linkend=\"sec-deployment-instserver-sles9\"/> )."
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:90
#, no-c-format
msgid "Run the following command to create the minimal ISO image:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:94
#, no-c-format
msgid "&prompt.sudo;<command>mksusecd</command> --create <replaceable>min-install.iso</replaceable> \\\n"
      "--net=nfs://<replaceable>192.168.1.1:/srv/install/</replaceable><replaceable>ARCH</replaceable>/<replaceable>OS_VERSION</replaceable>/<replaceable>SP_VERSION</replaceable>/cd1  \\\n"
      "/srv/tftpboot/EFI/<replaceable>ARCH</replaceable>/boot"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:98
#, no-c-format
msgid "Replace the NFS server address with your own. Replace <replaceable>ARCH</replaceable> with the directory corresponding to the target system architecture. Also replace <replaceable>OS_version</replaceable> and <replaceable>SP_VERSION</replaceable> (service pack) according to your paths in <xref linkend=\"sec-deployment-instserver-sles9\"/> ."
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:110
#, no-c-format
msgid "Instead of using an NFS server for the <option>--net</option> option, it is also possible to use an HTTP repository, for example the &opensuse; repository:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:116
#, no-c-format
msgid "&prompt.sudo;<command>mksusecd</command> --create <replaceable>min-install.iso</replaceable> \\\n"
      "--net=http://download.opensuse.org/tumbleweed/repo/oss/suse \\\n"
      "/srv/tftpboot/EFI/<replaceable>ARCH</replaceable>/boot"
msgstr ""

#. Tag: title
#: deployment_mksusecd.xml:122
#, no-c-format
msgid "Setting default kernel boot parameters"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:124
#, no-c-format
msgid "Rather than waiting for a boot prompt to enter your custom kernel boot parameters, configure them in a custom <command>mksusecd</command> image:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:129
#, no-c-format
msgid "&prompt.sudo;mksusecd --create <replaceable>install.iso</replaceable> \\\n"
      "--boot \"<replaceable>textmode=1 splash=silent mitigations=auto</replaceable>\""
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:132
#, no-c-format
msgid "Verify that your custom parameters loaded correctly after start-up by querying <filename>/proc</filename> :"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:137
#, no-c-format
msgid "&prompt.user;cat /proc/cmdline"
msgstr ""

#. Tag: title
#: deployment_mksusecd.xml:140
#, no-c-format
msgid "Customizing modules, extensions, and repositories"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:142
#, no-c-format
msgid "&sle;&nbsp;15 supports Modules (not to be confused with kernel modules) and Extensions for different product components. These are add-ons to the default <literal>Basesystem</literal> , such as <literal>Development Tools</literal> , <literal>Desktop Applications</literal> , and &sle; <literal>Live Patching</literal> . For more information refer to the &modulesquick; guide."
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:151
#, no-c-format
msgid "With <command>mksusecd</command> you can create an installation image containing the additional Modules and Extensions you want. Start by querying existing images, like this example for &sle;&nbsp;&productnumber;:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:157
#, no-c-format
msgid "&prompt.sudo;mksusecd --list-repos &packagemedia;\n"
      "Repositories:\n"
      "  Basesystem-Module [&product-ga;.&product-sp;-0]\n"
      "  SUSE-CAP-Tools-Module [&product-ga;.&product-sp;-0]\n"
      "  Containers-Module [&product-ga;.&product-sp;-0]\n"
      "  Desktop-Applications-Module [&product-ga;.&product-sp;-0]\n"
      "  Development-Tools-Module [&product-ga;.&product-sp;-0]\n"
      "  HPC-Module [&product-ga;.&product-sp;-0]\n"
      "  Legacy-Module [&product-ga;.&product-sp;-0]\n"
      "  Live-Patching [&product-ga;.&product-sp;-0]\n"
      "  Public-Cloud-Module [&product-ga;.&product-sp;-0]\n"
      "  Python2-Module [&product-ga;.&product-sp;-0]\n"
      "  SAP-Applications-Module [&product-ga;.&product-sp;-0]\n"
      "  Server-Applications-Module [&product-ga;.&product-sp;-0]\n"
      "  Transactional-Server-Module [&product-ga;.&product-sp;-0]\n"
      "  Web-Scripting-Module [&product-ga;.&product-sp;-0]\n"
      "  SLEHA&product-ga;-SP&product-sp; [&product-ga;.&product-sp;-0]\n"
      "  SLE-&product-ga;-SP&product-sp;-HPC [&product-ga;.&product-sp;-0]\n"
      "  SLED&product-ga;-SP&product-sp; [&product-ga;.&product-sp;-0]\n"
      "  SLES&product-ga;-SP&product-sp; [&product-ga;.&product-sp;-0]\n"
      "  SLE-&product-ga;-SP&product-sp;-SAP [&product-ga;.&product-sp;-0]\n"
      "  SLEWE&product-ga;-SP&product-sp; [&product-ga;.&product-sp;-0]\n"
      "  [...]\n"
      ""
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:182
#, no-c-format
msgid "Create a new installation image that is built from the Modules, Extensions, and repositories that you select, and automatically enable them:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:187
#, no-c-format
msgid "&prompt.sudo;mksusecd --create <replaceable>myinstaller.iso</replaceable> --enable-repos auto \\\n"
      "--include-repos <replaceable>Basesystem-Module,Desktop-Applications-Module</replaceable> \\\n"
      "&packagemedia;"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:191
#, no-c-format
msgid "This example creates an image for installation from the internet. To create an image for offline installation, additionally add the repository of the base product, for example <literal>SLES&product-ga;-SP&product-sp;</literal> for &sles;."
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:198
#, no-c-format
msgid "&prompt.sudo;mksusecd --create <replaceable>myinstaller.iso</replaceable> --enable-repos auto \\\n"
      "   --include-repos <replaceable>SLES&product-ga;-SP&product-sp;,Basesystem-Module,Desktop-Applications-Module</replaceable> \\\n"
      "   &packagemedia;"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:202
#, no-c-format
msgid "Replace <option>--enable-repos auto</option> with <option>--enable-repos ask</option> to have the installer present a dialog for choosing modules."
msgstr ""

#. Tag: title
#: deployment_mksusecd.xml:209
#, no-c-format
msgid "&ay; control file"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:210
#, no-c-format
msgid "When using the <option>--enable-repos</option> option, <command>mksusecd</command> adds an <filename>add_on_products.xml</filename> file for use with &ay; to the new image. Modules in this file do not need to be listed in the in the &ay; control file."
msgstr ""

#. Tag: title
#: deployment_mksusecd.xml:219
#, no-c-format
msgid "Creating a minimal netinstall ISO"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:221
#, no-c-format
msgid "To create a minimal installation image to launch a network installation, use the <command>--nano</command> option:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:226
#, no-c-format
msgid "&prompt.sudo;mksusecd --create <replaceable>netinstall.iso</replaceable> \\\n"
      "--nano &installmedia;"
msgstr ""

#. Tag: title
#: deployment_mksusecd.xml:230
#, no-c-format
msgid "Change default repository"
msgstr ""

#. Tag: para
#: deployment_mksusecd.xml:232
#, no-c-format
msgid "To set a different repository, such as your own local repository, use the <command>--net</command> option:"
msgstr ""

#. Tag: screen
#: deployment_mksusecd.xml:237
#, no-c-format
msgid "&prompt.sudo;mksusecd --create <replaceable>localinstall.iso</replaceable> \\\n"
      "--net \"https://example.com/local\" &installmedia;"
msgstr ""

