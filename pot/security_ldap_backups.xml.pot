#. Tag: title
#: security_ldap_backups.xml:12
#, no-c-format
msgid "Backing up and restoring LDAP servers"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:14
#, no-c-format
msgid "&ds389; supports making offline and online backups. The <command>dsctl</command> command makes offline database backups, and the <command>dsconf</command> command makes online database backups. Back up the LDAP server configuration directory, to enable complete restoration in case of a major failure."
msgstr ""

#. Tag: title
#: security_ldap_backups.xml:23
#, no-c-format
msgid "Backing up the LDAP server configuration"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:24
#, no-c-format
msgid "Your LDAP server configuration is in the directory <filename>/etc/dirsrv/slapd-<replaceable>INSTANCE_NAME</replaceable></filename> . This directory contains certificates, keys, and the <filename>dse.ldif</filename> file. Make a compressed backup of this directory with the <command>tar</command> command:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:31
#, no-c-format
msgid "&prompt.sudo;<command>tar caf config_slapd-<replaceable>INSTANCE_NAME</replaceable>_$(date +%Y-%m-%d_%H-%M-%S).tar.gz /etc/dirsrv/slapd-<replaceable>INSTANCE_NAME</replaceable>/</command>"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:33
#, no-c-format
msgid "When running <command>tar</command> , you may see the harmless informational message <literal>tar: Removing leading `/' from member names</literal> ."
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:38
#, no-c-format
msgid "To restore a previous configuration, unpack it to the same directory:"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:43
#, no-c-format
msgid "To avoid overwriting an existing configuring, move it:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:46
#, no-c-format
msgid "&prompt.sudo;<command>old /etc/dirsrv/slapd-<replaceable>INSTANCE_NAME</replaceable>/</command>"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:49
#, no-c-format
msgid "Unpack the backup archive:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:52
#, no-c-format
msgid "&prompt.sudo;<command>tar -xvzf config_slapd-<replaceable>INSTANCE_NAME</replaceable>_<replaceable>DATE</replaceable>.tar.gz</command>"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:55
#, no-c-format
msgid "Copy it to <filename>/etc/dirsrv/slapd-<replaceable>INSTANCE_NAME</replaceable></filename> :"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:59
#, no-c-format
msgid "&prompt.sudo;<command>cp -r <replaceable>etc/dirsrv/slapd-INSTANCE_NAME</replaceable> /etc/dirsrv/slapd-<replaceable>INSTANCE_NAME</replaceable></command>"
msgstr ""

#. Tag: title
#: security_ldap_backups.xml:65
#, no-c-format
msgid "Creating an offline backup of the LDAP database and restoring from it"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:66
#, no-c-format
msgid "The <command>dsctl</command> command makes offline backups. Stop the server:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:69
#, no-c-format
msgid "&prompt.sudo;<command>dsctl <replaceable>INSTANCE_NAME</replaceable> stop</command>\n"
      "Instance \"<replaceable>INSTANCE_NAME</replaceable>\" has been stopped"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:71
#, no-c-format
msgid "Then make the backup using your instance name:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:74
#, no-c-format
msgid "&prompt.sudo;<command>dsctl <replaceable>INSTANCE_NAME</replaceable> db2bak</command>\n"
      "db2bak successful"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:76
#, no-c-format
msgid "This example creates a backup archive at <filename>/var/lib/dirsrv/slapd-INSTANCE_NAME/bak/INSTANCE_NAME-2021_07_26_13_03_17</filename> ."
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:80
#, no-c-format
msgid "Restore from this backup, naming the directory containing the backup archive:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:84
#, no-c-format
msgid "&prompt.sudo;<command>dsctl <replaceable>INSTANCE_NAME</replaceable> bak2db <replaceable>/var/lib/dirsrv/slapd-INSTANCE_NAME/bak/INSTANCE_NAME-2021_07_26_13_03_17/</replaceable></command>\n"
      "bak2db successful"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:86
#, no-c-format
msgid "Then start the server:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:89
#, no-c-format
msgid "&prompt.sudo;<command>dsctl <replaceable>INSTANCE_NAME</replaceable> start</command>\n"
      "Instance \"<replaceable>INSTANCE_NAME</replaceable>\" has been started"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:91
#, no-c-format
msgid "You can also create LDIF backups:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:94
#, no-c-format
msgid "&prompt.sudo;<command>dsctl <replaceable>INSTANCE_NAME</replaceable> db2ldif --replication userRoot</command>\n"
      "ldiffile: <replaceable>/var/lib/dirsrv/slapd-INSTANCE_NAME/ldif/INSTANCE_NAME-userRoot-2021_07_28_08_47_30.ldif</replaceable>\n"
      "db2ldif successful"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:97
#, no-c-format
msgid "To restore an LDIF backup with the name of the archive, then start the server:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:100
#, no-c-format
msgid "&prompt.sudo;<command>dsctl ldif2db userRoot <replaceable>/var/lib/dirsrv/slapd-INSTANCE_NAME/ldif/INSTANCE_NAME-userRoot-2021_07_28_08_47_30.ldif</replaceable></command>\n"
      "&prompt.sudo;<command>dsctl INSTANCE_NAME start</command>"
msgstr ""

#. Tag: title
#: security_ldap_backups.xml:105
#, no-c-format
msgid "Creating an online backup of the LDAP database and restoring from it"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:106
#, no-c-format
msgid "Use the <command>dsconf</command> to make an online backup of your LDAP database:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:110
#, no-c-format
msgid "&prompt.sudo;<command>dsconf <replaceable>INSTANCE_NAME</replaceable> backup create</command>\n"
      "The backup create task has finished successfully"
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:112
#, no-c-format
msgid "This creates <replaceable>/var/lib/dirsrv/slapd-INSTANCE_NAME/bak/INSTANCE_NAME-2021_07_28_09_46_08</replaceable> ."
msgstr ""

#. Tag: para
#: security_ldap_backups.xml:116
#, no-c-format
msgid "Restore it:"
msgstr ""

#. Tag: screen
#: security_ldap_backups.xml:119
#, no-c-format
msgid "&prompt.sudo;<command>dsconf <replaceable>INSTANCE_NAME</replaceable> backup restore <replaceable>/var/lib/dirsrv/slapd-INSTANCE_NAME/bak/INSTANCE_NAME-2021_07_28_09_46_08</replaceable></command>"
msgstr ""

