      "--config <replaceable>config-4.12.14-lp151.28.13-default</replaceable> \\\n"
      "--map <replaceable>System.map-4.12.14-lp151.28.13-default</replaceable>| tee <replaceable>filename.txt</replaceable>\n"
      "    "
