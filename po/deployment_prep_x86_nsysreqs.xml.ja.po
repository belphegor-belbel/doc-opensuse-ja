#: deployment_prep_x86_nsysreqs.xml:88
#, no-c-format
msgid "Installation Scope"
msgstr ""

#. Tag: para
#: deployment_prep_x86_nsysreqs.xml:93
#, no-c-format
msgid "Minimum Hard Disk Requirements"
msgstr ""

