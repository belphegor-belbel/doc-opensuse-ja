msgstr "/etc/host.conf で使用するパラメータ"

#. Tag: para
#: net_config_files.xml:515
#, no-c-format
msgid "order <emphasis>hosts</emphasis> , <emphasis>bind</emphasis>"
msgstr "order <emphasis>hosts</emphasis> , <emphasis>bind</emphasis>"

#. Tag: para
#: net_config_files.xml:520
#, no-c-format
msgid ""
