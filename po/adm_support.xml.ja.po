msgid "Enabling Archiving Mode in the SCA Appliance"
msgstr ""

#. Tag: para
#: adm_support.xml:1739 adm_support.xml:1828
#, no-c-format
msgid "Open <filename>/etc/sca/sdagent.conf</filename> in an editor."
msgstr ""

#. Tag: screen
#: adm_support.xml:1747
#, no-c-format
msgid "ARCHIVE_MODE=0"
msgstr ""

#. Tag: screen
#: adm_support.xml:1751
#, no-c-format
msgid "ARCHIVE_MODE=1"
msgstr ""

#. Tag: para
#: adm_support.xml:1760
#, no-c-format
msgid ""
