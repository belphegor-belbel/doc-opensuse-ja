msgstr "BOOTP リクエストに対するタイムアウト (この例では 10 秒) を指定します。"

#. Tag: para
#: ay_bigfile.xml:18032
#, no-c-format
msgid "<literal>bootpwait: 5</literal>"
msgstr "<literal>bootpwait: 5</literal>"

#. Tag: para
#: ay_bigfile.xml:18037
#, no-c-format
