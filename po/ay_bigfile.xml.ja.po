msgstr "&lt;cache_mode&gt;writethrough&lt;/cache_mode&gt;"

#. Tag: para
#: ay_bigfile.xml:4718
#, no-c-format
msgid ""
"Cache mode for &bcache;. Possible values are <literal>writethrough</"
"literal> , <literal>writeback</literal> , <literal>writearound</literal> and "
"<literal>none</literal> ."
msgstr ""
