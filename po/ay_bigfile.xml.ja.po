"   ...\n"
"  &lt;/storage&gt;\n"
"  &lt;wait&gt;<co xml:id=\"co.ay.general.wait\"/>\n"
"   ...\n"
"  &lt;/wait&gt;\n"
" &lt;/general&gt;\n"
"&lt;profile&gt;"
msgstr ""
"&lt;?xml version=\"1.0\"?&gt;\n"
"&lt;!DOCTYPE profile&gt;\n"
"&lt;profile xmlns=\"http://www.suse.com/1.0/yast2ns\"\n"
