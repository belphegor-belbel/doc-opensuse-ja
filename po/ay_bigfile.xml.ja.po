"  &lt;nis_broken_server config:type=\"boolean\"&gt;true&lt;/"
"nis_broken_server&gt;\n"
"  &lt;nis_by_dhcp config:type=\"boolean\"&gt;false&lt;/nis_by_dhcp&gt;\n"
"  &lt;nis_domain&gt;test.com&lt;/nis_domain&gt;\n"
