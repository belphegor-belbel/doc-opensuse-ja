"    true\n"
"   </accept_verification_failed>\n"
"   <accept_unknown_gpg_key config:type=\"boolean\">\n"
"    true\n"
"   </accept_unknown_gpg_key>\n"
"   <import_gpg_key config:type=\"boolean\">true</import_gpg_key>\n"
