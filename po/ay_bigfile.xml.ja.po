"   ...\n"
"  &lt;/ask-list&gt;\n"
"  &lt;cio_ignore&gt;<co xml:id=\"co.ay.general.cio_ignore\" arch=\"zseries\"/"
">\n"
"   ...\n"
"  &lt;/cio_ignore&gt;\n"
"  &lt;mode&gt;<co xml:id=\"co.ay.general.mode\"/>\n"
"   ...\n"
"  &lt;/mode&gt;\n"
"  &lt;proposals&gt;<co xml:id=\"co.ay.general.proposal\"/>\n"
"   ...\n"
"  &lt;/proposals&gt;\n"
"  &lt;self_update&gt;<co xml:id=\"co.ay.general.self_update\"/>\n"
"   ...\n"
"  &lt;/self_update&gt;\n"
"  &lt;self_update_url&gt;\n"
"   ...\n"
"  &lt;/self_update_url&gt;\n"
"  &lt;semi-automatic config:type=\"list\"&gt;<co xml:id=\"co.ay.general.semi"
"\"/>\n"
"   ...\n"
"  &lt;/semi-automatic&gt;\n"
