"    &lt;/nfs_entry&gt;\n"
"  &lt;/nfs_entries&gt;\n"
"&lt;/nfs&gt;"
msgstr ""
"&lt;nfs&gt;\n"
"  &lt;enable_nfs4 config:type=\"boolean\"&gt;true&lt;/enable_nfs4&gt;\n"
"  &lt;idmapd_domain&gt;suse.cz&lt;/idmapd_domain&gt;\n"
"  &lt;nfs_entries config:type=\"list\"&gt;\n"
"    &lt;nfs_entry&gt;\n"
"      &lt;mount_point&gt;/home&lt;/mount_point&gt;\n"
