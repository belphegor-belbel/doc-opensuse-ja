#: xm_vs_xl.xml:63
#, no-c-format
msgid "Meaning"
msgstr "意味"

#. Tag: para
#: xm_vs_xl.xml:72
#, no-c-format
msgid "(-) minus"
msgstr "(-) マイナス"

