"Enter LDAP Password: <replaceable>PASSWORD</replaceable>\n"
"dn: <replaceable>uid=&exampleuserII_plain;,ou=people,dc=LDAP1,dc=COM</"
"replaceable>\n"
"    "
msgstr ""
"\n"
"&prompt.user;<command>ldapwhoami -D uid=&exampleuserII_plain;,ou=people,"
"dc=<replaceable>LDAP1</replaceable>,dc=<replaceable>COM</replaceable> -W</"
"command>\n"
"Enter LDAP Password: <replaceable>PASSWORD</replaceable>\n"
"dn: uid=&exampleuserII_plain;,ou=people,dc=<replaceable>LDAP1</replaceable>,"
"dc=<replaceable>COM</replaceable>\n"
"     "

