"\n"
"EXPOSE 80 <co xml:id=\"co-dockerfile-apache2-expose\"/>\n"
"\n"
"ENTRYPOINT [\"/usr/sbin/httpd\"]\n"
"CMD [\"-D\", \"FOREGROUND\"]"
msgstr ""

