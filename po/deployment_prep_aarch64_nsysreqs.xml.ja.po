#: deployment_prep_aarch64_nsysreqs.xml:117
#, no-c-format
msgid "Minimal System"
msgstr ""

#. Tag: para
#: deployment_prep_aarch64_nsysreqs.xml:122
#, no-c-format
msgid "2.5 GB"
msgstr ""

#. Tag: para
#: deployment_prep_aarch64_nsysreqs.xml:129
#, no-c-format
