#: deployment_prep_aarch64_nsysreqs.xml:105
#, no-c-format
msgid "Text Mode"
msgstr ""

#. Tag: para
#: deployment_prep_aarch64_nsysreqs.xml:110
#, no-c-format
msgid "1.5 GB"
msgstr ""

