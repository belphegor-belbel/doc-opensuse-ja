#: rmt_scripts.xml:62
#, no-c-format
msgid "Mirror repositories."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:68
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli import</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:70
#, no-c-format
msgid "Import commands for the offline mode."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:76
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli export</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:78
#, no-c-format
msgid "Export commands for the offline mode."
msgstr ""

