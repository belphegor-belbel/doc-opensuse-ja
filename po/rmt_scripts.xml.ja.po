#. Tag: para
#: rmt_scripts.xml:92
#, no-c-format
msgid "The following sections explain each sub-command in detail."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:97
#, no-c-format
msgid ""
"\"\n"
"  <command>sync</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:98
#, no-c-format
msgid "This command triggers the synchronization with the &scc; instantly. The command has no further options. The synchronization is also triggered each night by th &systemd; timer <systemitem>rmt-server-sync.timer</systemitem> ."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:107
#, no-c-format
msgid ""
"\"\n"
"  <command>products</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:108
#, no-c-format
msgid "List and modify products."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:113
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli products list [--all] [--csv]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:115
#, no-c-format
msgid "Lists the products that are enabled for mirroring. Use the <option>--all</option> flag to list all available products. Use the <option>--csv</option> flag to output the list in CSV format. <option>ls</option> can be used as a shortcut for <option>list</option> ."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:125
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli products enable [id | string]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:127
#, no-c-format
msgid "Enables mandatory repositories of a product by its id or product string."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:133
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli products disable [id | string]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:135
#, no-c-format
msgid "Disables all repositories of a product by its id or product string."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:144
#, no-c-format
msgid ""
"\"\n"
"  <command>repos</command>\n"
"\""
msgstr ""

#. Tag: term
#: rmt_scripts.xml:147
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos list [--all] [--csv]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:149
#, no-c-format
msgid "Lists the repositories that are enabled for mirroring. Use the <option>--all</option> flag to list all available repositories. Use the <option>--csv</option> flag to output the list in CSV format. <option>ls</option> can be used as a shortcut for <option>list</option> ."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:159
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos enable [id]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:161
#, no-c-format
msgid "Enables a single repository by its id."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:170
#, no-c-format
msgid ""
"\"\n"
"  <command>repos custom</command>\n"
"\""
msgstr ""

#. Tag: term
#: rmt_scripts.xml:173
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos disable [id]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:175
#, no-c-format
msgid "Disables a single repository by its id."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:181
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos custom list [--csv]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:183
#, no-c-format
msgid "Lists all your custom repositories. Use the <option>--csv</option> flag to output the list in CSV format. <option>ls</option> can be used as a shortcut for <option>list</option> ."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:192
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos custom add [url] [name]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:194
#, no-c-format
msgid "Adds a new custom repository, for example: <command> rmt-cli repos custom add https://download.opensuse.org/repositories/Virtualization:/containers/SLE_12_SP3/ Virtualization:Containers </command>"
msgstr ""

#. Tag: term
#: rmt_scripts.xml:203
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos custom enable [id]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:205
#, no-c-format
msgid "Enables mirroring for a custom repository."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:211
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos custom disable [id]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:213
#, no-c-format
msgid "Disables mirroring for a custom repository."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:219
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos custom remove [id]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:221
#, no-c-format
msgid "Removes a custom repository."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:227
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos custom products [id]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:229
#, no-c-format
msgid "Lists the products attached to the custom repository with given id."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:235
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos custom attach [id] [product id]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:237
#, no-c-format
msgid "Attaches an existing custom repository to a product."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:243
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli repos custom detach [id] [product id]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:245
#, no-c-format
msgid "Detaches an existing custom repository from a product."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:254
#, no-c-format
msgid ""
"\"\n"
"  <command>mirror</command>\n"
"\""
msgstr ""

#. Tag: term
#: rmt_scripts.xml:257
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli mirror</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:259
#, no-c-format
msgid "In its default configuration, &rmt; mirrors its enabled product repositories automatically once every night. This command starts the mirroring process manually."
msgstr ""

#. Tag: para
#: rmt_scripts.xml:264
#, no-c-format
msgid "When all enabled repositories are fully mirrored, you can register your client systems against &rmt; by running <command>SUSEConnect --url https://<replaceable>&rmt;_HOSTNAME</replaceable></command> on the client machine. After successful registration the repositories from &rmt; will be used by zypper on the client machine."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:279
#, no-c-format
msgid ""
"\"\n"
"  <command>import</command>\n"
"\""
msgstr ""

#. Tag: term
#: rmt_scripts.xml:287
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli import data [path]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:289
#, no-c-format
msgid "Run this on the offline &rmt; to read the JSON files from given path and fill the local database with data."
msgstr ""

#. Tag: para
#: rmt_scripts.xml:298
#, no-c-format
msgid "Run this on the offline &rmt; to import RPM packages."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:307
#, no-c-format
msgid ""
"\"\n"
"  <command>export</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:308
#, no-c-format
msgid "This command is required for the <emphasis>offline mode</emphasis> . For details, see <xref linkend=\"sec-rmt_mirroring-export_import\"/> ."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:315
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli export data [path]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:317
#, no-c-format
msgid "Run this on an online &rmt; to get the latest data from SUSE Customer Center and save it as JSON files at the specified path."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:323
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli export settings [path]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:325
#, no-c-format
msgid "Run this on the offline &rmt; to save the settings for enabled repositories at given path as <filename>repos.json</filename> ."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:331
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-cli import repos [path]</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:333
#, no-c-format
msgid "Run this on the offline &rmt; to export RPM packages."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:342
#, no-c-format
msgid ""
"\"\n"
"  <command>version</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:343
#, no-c-format
msgid "Display the version of <command>rmt-cli</command> ."
msgstr ""

#. Tag: title
#: rmt_scripts.xml:350
#, no-c-format
msgid "&rmt; &systemd; Commands"
msgstr ""

#. Tag: para
#: rmt_scripts.xml:351
#, no-c-format
msgid "You can manage &rmt;-related services with the standard &systemd; commands. The &rmt; server has the following services and timers:"
msgstr ""

#. Tag: term
#: rmt_scripts.xml:357
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-server.target</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:360
#, no-c-format
msgid "A &systemd; target that starts all required &rmt; components."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:366
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-server.service</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:369
#, no-c-format
msgid "The &rmt; server."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:375
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-server-migration.service</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:378
#, no-c-format
msgid "This server migrates the database to the newest schema, if required. There is no need to manually interact with this service."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:385
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-server-sync.timer</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:388
#, no-c-format
msgid "This timer is responsible for periodically synchronizing all repository product data from the &scc;."
msgstr ""

#. Tag: term
#: rmt_scripts.xml:395
#, no-c-format
msgid ""
"\"\n"
"  <command>rmt-server-mirror.timer</command>\n"
"\""
msgstr ""

#. Tag: para
#: rmt_scripts.xml:398
#, no-c-format
msgid "This timer is responsible for periodically synchronizing all RPMs from the &scc;."
msgstr ""

#. Tag: para
#: rmt_scripts.xml:405
#, no-c-format
msgid "Use <command>systemctl</command> to control the &rmt; services and timers."
