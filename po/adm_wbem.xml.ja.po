msgstr ""

#. Tag: para
#: adm_wbem.xml:667
#, no-c-format
msgid ""
"Specifies whether SFCB should accept HTTP client connections. Default is "
"<literal>false</literal> ."
msgstr ""

#. Tag: para
#: adm_wbem.xml:672
#, no-c-format
msgid "<literal>enableHttp:</literal> <replaceable>OPTION</replaceable>"
msgstr ""

#. Tag: para
#: adm_wbem.xml:680 adm_wbem.xml:792 adm_wbem.xml:861 adm_wbem.xml:933
#: adm_wbem.xml:1005 adm_wbem.xml:1139 adm_wbem.xml:1245
