#~ "add_crash_kernel&gt;\n"
#~ "  &lt;crash_kernel config:type=\"list\"&gt;\n"
#~ "    &lt;listentry&gt;72M,low&lt;/listentry&gt;\n"
#~ "    &lt;listentry&gt;256M,high&lt;/listentry&gt;\n"
#~ "  &lt;/crash_kernel&gt;\n"
#~ "&lt;/kdump&gt; "
#~ msgstr ""
